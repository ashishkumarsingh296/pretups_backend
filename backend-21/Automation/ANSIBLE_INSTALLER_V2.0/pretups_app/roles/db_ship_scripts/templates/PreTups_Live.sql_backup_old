-- SERVICE_TYPE  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.SERVICE_TYPE ( SERVICE_TYPE VARCHAR2(10 BYTE) NOT NULL, MODULE VARCHAR2(10 BYTE) NOT NULL, TYPE VARCHAR2(20 BYTE) NOT NULL, MESSAGE_FORMAT VARCHAR2(100 BYTE) NULL, REQUEST_HANDLER VARCHAR2(150 BYTE) NOT NULL, ERROR_KEY VARCHAR2(50 BYTE) NOT NULL, DESCRIPTION NVARCHAR2(100) NULL, FLEXIBLE VARCHAR2(1 BYTE) NOT NULL, CREATED_ON DATE NOT NULL, CREATED_BY VARCHAR2(20 BYTE) NOT NULL, MODIFIED_ON DATE NOT NULL, MODIFIED_BY VARCHAR2(20 BYTE) NOT NULL, NAME NVARCHAR2(50) NULL, EXTERNAL_INTERFACE VARCHAR2(1 BYTE) DEFAULT 'N' NOT NULL, UNREGISTERED_ACCESS_ALLOWED VARCHAR2(1 BYTE) DEFAULT 'N' NOT NULL, STATUS VARCHAR2(1 BYTE) DEFAULT 'Y' NULL, SEQ_NO NUMBER(5) NULL, USE_INTERFACE_LANGUAGE VARCHAR2(1 BYTE) DEFAULT 'N' NULL, GROUP_TYPE VARCHAR2(10 BYTE) DEFAULT 'NA' NOT NULL, SUB_KEYWORD_APPLICABLE VARCHAR2(1 BYTE) DEFAULT 'N' NOT NULL, FILE_PARSER VARCHAR2(150 BYTE) NULL, PRIMARY KEY (SERVICE_TYPE) USING INDEX TABLESPACE {{ P2P_INDX_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SERVICE_TYPE.SERVICE_TYPE IS 'Unique Service type'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SERVICE_TYPE.MODULE IS 'PreTUPs module, P2P or C2S'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SERVICE_TYPE.TYPE IS 'Type of the service, POSTPAID, PREPAID, EVD, OTHER'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SERVICE_TYPE.MESSAGE_FORMAT IS 'request message format'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SERVICE_TYPE.REQUEST_HANDLER IS 'request handler class name'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SERVICE_TYPE.ERROR_KEY IS 'error key, error message will be picked from the resource file'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SERVICE_TYPE.DESCRIPTION IS 'Description of the service type'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SERVICE_TYPE.NAME IS 'service type, will be used to display in combo'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SERVICE_TYPE.EXTERNAL_INTERFACE IS 'Y if service had external interface like, IN or Billing System, Voucher Distribution'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SERVICE_TYPE.UNREGISTERED_ACCESS_ALLOWED IS 'Whether to allow service for unregistered users'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SERVICE_TYPE.FILE_PARSER IS 'File parser for batch recharge, this parser for different for each service type.'
/


