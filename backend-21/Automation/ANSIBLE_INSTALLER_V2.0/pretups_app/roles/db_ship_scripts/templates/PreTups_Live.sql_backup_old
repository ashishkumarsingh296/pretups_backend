--
-- P2P_BUDDIES  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_BUDDIES ( BUDDY_MSISDN VARCHAR2(15 BYTE) NOT NULL, PARENT_ID VARCHAR2(20 BYTE) NOT NULL, BUDDY_SEQ_NUM NUMBER(3) NULL, BUDDY_NAME VARCHAR2(30 BYTE) NOT NULL, STATUS VARCHAR2(1 BYTE) NULL, BUDDY_LAST_TRANSFER_ID VARCHAR2(20 BYTE) NULL, BUDDY_LAST_TRANSFER_ON DATE NULL, BUDDY_LAST_TRANSFER_TYPE VARCHAR2(10 BYTE) NULL, BUDDY_TOTAL_TRANSFER NUMBER(16) DEFAULT 0 NULL, BUDDY_TOTAL_TRANSFER_AMT NUMBER(20) DEFAULT 0 NULL, CREATED_ON DATE NOT NULL, CREATED_BY VARCHAR2(20 BYTE) NOT NULL, MODIFIED_ON DATE NOT NULL, MODIFIED_BY VARCHAR2(20 BYTE) NOT NULL, PREFERRED_AMOUNT NUMBER(20) NOT NULL, LAST_TRANSFER_AMOUNT NUMBER(20) DEFAULT 0 NULL, PREFIX_ID NUMBER(6) NOT NULL, CONSTRAINT PK_P2P_BUDDIES PRIMARY KEY (PARENT_ID, BUDDY_MSISDN) USING INDEX TABLESPACE {{ P2P_INDX_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ), CONSTRAINT UK_P2P_BUDDIES UNIQUE (BUDDY_NAME, PARENT_ID) USING INDEX TABLESPACE {{ P2P_INDX_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 64K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ), FOREIGN KEY (PARENT_ID) REFERENCES {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_SUBSCRIBERS (USER_ID) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_BUDDIES.BUDDY_MSISDN IS 'Buddy MSISDN in the subscriber list'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_BUDDIES.PARENT_ID IS 'User ID under which this Buddy is registered'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_BUDDIES.BUDDY_SEQ_NUM IS 'Seq num of the buddy in the subscriber'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_BUDDIES.BUDDY_NAME IS 'Name of the buddy specified at the time of registration'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_BUDDIES.STATUS IS 'Status of the Buddy Active etc'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_BUDDIES.BUDDY_LAST_TRANSFER_ID IS 'Last transfer ID to this Buddy User'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_BUDDIES.BUDDY_LAST_TRANSFER_ON IS 'Date time of the last transfer to this buddy'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_BUDDIES.BUDDY_LAST_TRANSFER_TYPE IS 'Type of Last transfer to this buddy'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_BUDDIES.BUDDY_TOTAL_TRANSFER IS 'Total transfers to this buddy till now'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_BUDDIES.BUDDY_TOTAL_TRANSFER_AMT IS 'Total transfer amount to this buddy till now'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_BUDDIES.CREATED_ON IS 'Date time when this buddy was added in the subscriber list'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_BUDDIES.CREATED_BY IS 'User ID of the user who created this buddy'
