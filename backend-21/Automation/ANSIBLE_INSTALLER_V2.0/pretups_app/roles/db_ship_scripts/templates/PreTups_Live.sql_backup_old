-- BARRED_MSISDNS  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.BARRED_MSISDNS ( MODULE VARCHAR2(10 BYTE) NOT NULL, NETWORK_CODE VARCHAR2(2 BYTE) NOT NULL, MSISDN VARCHAR2(15 BYTE) NOT NULL, NAME NVARCHAR2(60) NULL, USER_TYPE VARCHAR2(15 BYTE) NULL, BARRED_TYPE VARCHAR2(10 BYTE) NULL, CREATED_ON DATE NOT NULL, CREATED_BY VARCHAR2(20 BYTE) NOT NULL, MODIFIED_ON DATE NOT NULL, MODIFIED_BY VARCHAR2(20 BYTE) NOT NULL, BARRED_REASON NVARCHAR2(100) NULL, CREATED_DATE DATE NULL, CONSTRAINT PK_BARRED_MSISDNS PRIMARY KEY (MODULE, NETWORK_CODE, MSISDN, USER_TYPE, BARRED_TYPE) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

