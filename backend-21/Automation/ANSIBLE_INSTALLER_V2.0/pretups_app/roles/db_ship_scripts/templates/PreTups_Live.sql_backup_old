--
-- INVALID_STATUS_CHANGE  (Trigger) 
--
CREATE OR REPLACE TRIGGER {{ PRETUPS_SCHEMA_USER_NAME }}.Invalid_status_change  
	AFTER UPDATE OF status ON voms_vouchers  
	FOR EACH ROW
DECLARE  
	Invalid_status_change EXCEPTION;  
	BEGIN  
	IF (:old.status IN ('CU','ST','DA') AND :old.status <> :new.status ) THEN  
	RAISE Invalid_status_change;  
	END IF;  
	END;
/
SHOW ERRORS;




--
-- NETWORK_PREFERENCES  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_PREFERENCES ( NETWORK_CODE VARCHAR2(2 BYTE) NOT NULL, PREFERENCE_CODE VARCHAR2(45 BYTE) NOT NULL, VALUE VARCHAR2(100 BYTE) NULL, CREATED_ON DATE NOT NULL, CREATED_BY VARCHAR2(20 BYTE) NOT NULL, MODIFIED_ON DATE NOT NULL, MODIFIED_BY VARCHAR2(20 BYTE) NOT NULL, PRIMARY KEY (NETWORK_CODE, PREFERENCE_CODE) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ), FOREIGN KEY (PREFERENCE_CODE) REFERENCES {{ PRETUPS_SCHEMA_USER_NAME }}.SYSTEM_PREFERENCES (PREFERENCE_CODE), FOREIGN KEY (NETWORK_CODE) REFERENCES {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORKS (NETWORK_CODE) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_PREFERENCES.NETWORK_CODE IS 'Pretups Network code'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_PREFERENCES.PREFERENCE_CODE IS 'network Preference code'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_PREFERENCES.VALUE IS 'network preference value'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_PREFERENCES.CREATED_ON IS 'network preference created on'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_PREFERENCES.CREATED_BY IS 'network preference created by'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_PREFERENCES.MODIFIED_ON IS 'network preference modified on'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_PREFERENCES.MODIFIED_BY IS 'network preference modified by'
/


--
-- NETWORK_LOAD  (Table) 
--

CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_LOAD
(
  INSTANCE_ID       VARCHAR2(5 BYTE)            NOT NULL,
  NETWORK_CODE      VARCHAR2(2 BYTE)            NOT NULL,
  NETWORK_LOAD      NUMBER(3),
  REQUEST_TIME_OUT  NUMBER(7),
  MIN_PROCESS_TIME  NUMBER(5),
  CREATED_ON        DATE                        NOT NULL,
  CREATED_BY        VARCHAR2(20 BYTE)           NOT NULL,
  MODIFIED_ON       DATE                        NOT NULL,
  MODIFIED_BY       VARCHAR2(20 BYTE)           NOT NULL,
  NETWORK_TPS       NUMBER(5),
  C2S_INSTANCE_ID   VARCHAR2(5 BYTE)            NOT NULL,
  P2P_INSTANCE_ID   VARCHAR2(5 BYTE)            NOT NULL
)
TABLESPACE {{ P_C2SINDX1_TS_NAME }}
PCTUSED    40
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          160K
            NEXT             16K
            MINEXTENTS       1
            MAXEXTENTS       505
            PCTINCREASE      50
            FREELISTS        1
            FREELIST GROUPS  1
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_LOAD ( INSTANCE_ID VARCHAR2(5 BYTE) NOT NULL, NETWORK_CODE VARCHAR2(2 BYTE) NOT NULL, NETWORK_LOAD NUMBER(3) NULL, REQUEST_TIME_OUT NUMBER(7) NULL, MIN_PROCESS_TIME NUMBER(5) NULL, CREATED_ON DATE NOT NULL, CREATED_BY VARCHAR2(20 BYTE) NOT NULL, MODIFIED_ON DATE NOT NULL, MODIFIED_BY VARCHAR2(20 BYTE) NOT NULL, NETWORK_TPS NUMBER(5) NULL, C2S_INSTANCE_ID VARCHAR2(5 BYTE) NOT NULL, P2P_INSTANCE_ID VARCHAR2(5 BYTE) NOT NULL, PRIMARY KEY (INSTANCE_ID, NETWORK_CODE) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 , FOREIGN KEY (INSTANCE_ID) REFERENCES {{ PRETUPS_SCHEMA_USER_NAME }}.INSTANCE_LOAD (INSTANCE_ID) ) TABLESPACE {{ PRTP_DATA_TS_NAME }}  LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_LOAD.INSTANCE_ID IS 'System Generated ID of the Application Server'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_LOAD.NETWORK_CODE IS 'Network Code that are operating in the Instance'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_LOAD.NETWORK_LOAD IS 'Load that can be handled by the network in that instance'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_LOAD.REQUEST_TIME_OUT IS 'Request time out time after which the request will get Time out'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_LOAD.MIN_PROCESS_TIME IS 'Minimum Process time that is required for the transaction to complete for this network'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_LOAD.CREATED_ON IS 'Date time when this entry was created'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_LOAD.CREATED_BY IS 'User ID of the user who created the entry'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_LOAD.MODIFIED_ON IS 'Date time when this entry was last modified'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_LOAD.MODIFIED_BY IS 'User ID of the user who last modified the entry'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_LOAD.NETWORK_TPS IS 'Transaction per second that can handled by the network in this instance'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_LOAD.C2S_INSTANCE_ID IS 'This ID will be used in case of C2S Web recharge Request. This ID will be used to get the Host Address and Port to which request will be sent.'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_LOAD.P2P_INSTANCE_ID IS 'This ID will be used in case of P2P Web credit transfer (if we give)Request. This ID will be used to get the Host Address and Port to which request will be sent.'
/


--
-- SUBSCRIBER_SELECTOR_MAPPING  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_SELECTOR_MAPPING ( SNO VARCHAR2(10 BYTE) NOT NULL, MAPPING_TYPE VARCHAR2(10 BYTE) NOT NULL, SENDER_SUBSCRIBER_TYPE VARCHAR2(10 BYTE) NOT NULL, RECEIVER_SUBSCRIBER_TYPE VARCHAR2(10 BYTE) NOT NULL, STATUS VARCHAR2(1 BYTE) DEFAULT 'Y' NOT NULL, CREATED_BY VARCHAR2(20 BYTE) NOT NULL, CREATED_ON DATE NOT NULL, MODIFIED_BY VARCHAR2(20 BYTE) NOT NULL, MODIFIED_ON DATE NOT NULL, CONSTRAINT PK_SUB_SEL_MAPPING PRIMARY KEY (SNO, MAPPING_TYPE, SENDER_SUBSCRIBER_TYPE, RECEIVER_SUBSCRIBER_TYPE) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 64K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ), CONSTRAINT FK_SUB_SEL_MAPPING FOREIGN KEY (SNO) REFERENCES {{ PRETUPS_SCHEMA_USER_NAME }}.SERVICE_TYPE_SELECTOR_MAPPING (SNO) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 64K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_SELECTOR_MAPPING.SNO IS 'Serial No from Service Type selector mapping table'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_SELECTOR_MAPPING.MAPPING_TYPE IS 'Mapping type, can be used for future'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_SELECTOR_MAPPING.SENDER_SUBSCRIBER_TYPE IS 'Sender subscriber type (PRE/POST) , in C2S case  Sender service type define as ALL'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_SELECTOR_MAPPING.RECEIVER_SUBSCRIBER_TYPE IS 'Receiver subscriber type (PRE/POST)'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_SELECTOR_MAPPING.STATUS IS 'Status of the combination'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_SELECTOR_MAPPING.CREATED_BY IS 'Subscriber Service type selector mapping created by'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_SELECTOR_MAPPING.CREATED_ON IS 'Subscriber Service type selector mapping created on'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_SELECTOR_MAPPING.MODIFIED_BY IS 'Subscriber Service type selector mapping modified by'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_SELECTOR_MAPPING.MODIFIED_ON IS 'Subscriber Service type selector mapping modified on'
/


--
-- VAS_ITEMS  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.VAS_ITEMS ( VAS_ITEM_ID VARCHAR2(15 BYTE) NOT NULL, VAS_ITEM_CODE VARCHAR2(6 BYTE) NOT NULL, VAS_CATEGORY_ID VARCHAR2(15 BYTE) NOT NULL, STATUS VARCHAR2(1 BYTE) NOT NULL, DESCRIPTION NVARCHAR2(50) NOT NULL, CREATED_ON DATE NOT NULL, CREATED_BY VARCHAR2(20 BYTE) NULL, MODIFIED_ON DATE NULL, MODIFIED_BY VARCHAR2(20 BYTE) NULL, REFERENCE VARCHAR2(10 BYTE) NULL, COUNTER_ID NUMBER(10) NULL, CONTENT_CODE VARCHAR2(10 BYTE) NULL, PROVIDER VARCHAR2(10 BYTE) NULL, EXT_PRODUCT_CODE VARCHAR2(15 BYTE) NULL, ACTIVATION_DATE DATE NULL, EXPIRY_DATE DATE NULL, BASE_PRICE NUMBER(10) NULL, RETAILER_TEMPLATE VARCHAR2(10 BYTE) NULL, TARGET_TEMPLATE VARCHAR2(10 BYTE) NULL, GIFTER_TEMPLATE VARCHAR2(10 BYTE) NULL, CREATED_BATCH_ID VARCHAR2(20 BYTE) NULL, MODIFIED_BATCH_ID VARCHAR2(20 BYTE) NULL, CREATION_TYPE VARCHAR2(1 BYTE) NULL, CONSTRAINT PK_VAS_ITEMS PRIMARY KEY (VAS_ITEM_ID) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 64K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ), CONSTRAINT UK_VAS_ITEMS UNIQUE (VAS_ITEM_CODE) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 64K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ), CONSTRAINT FK_VAS_ITEMS FOREIGN KEY (VAS_CATEGORY_ID) REFERENCES {{ PRETUPS_SCHEMA_USER_NAME }}.VAS_CATEGORIES (VAS_CATEGORY_ID) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 64K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/


--
-- VAS_CIRCLE_ITEM_PRICES  (Table) 
