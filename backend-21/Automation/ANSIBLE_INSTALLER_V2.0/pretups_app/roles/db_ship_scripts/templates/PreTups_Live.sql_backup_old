CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.MONTHLY_TRANSACTION_SUMMARY ( TRANS_MONTH NUMBER(2) NOT NULL, TRANS_MONTH_YEAR DATE NOT NULL, SENDER_SERVICE_CLASS VARCHAR2(10 BYTE) NOT NULL, RECEIVER_SERVICE_CLASS VARCHAR2(10 BYTE) NOT NULL, SERVICE VARCHAR2(10 BYTE) NOT NULL, SUCCESS_COUNT NUMBER(16) NULL, FAILURE_COUNT NUMBER(16) NULL, SUCCESS_AMT NUMBER(20) NULL, FAILURE_AMT NUMBER(30) NULL, SENDER_PROCESSING_FEE NUMBER(20) NULL, RECEIVER_PROCESSING_FEE NUMBER(20) NULL, SENDER_TAX1_AMOUNT NUMBER(20) NULL, SENDER_TAX2_AMOUNT NUMBER(20) NULL, RECEIVER_TAX1_AMOUNT NUMBER(20) NULL, RECEIVER_TAX2_AMOUNT NUMBER(20) NULL, SENDER_DEBIT_AMOUNT NUMBER(20) NULL, RECEIVER_CREDIT_AMOUNT NUMBER(20) NULL, SENDER_NETWORK_CODE VARCHAR2(2 BYTE) NOT NULL, RECEIVER_NETWORK_CODE VARCHAR2(2 BYTE) NOT NULL, CREATED_ON DATE NOT NULL, SUB_SERVICE VARCHAR2(10 BYTE) NULL, BONUS_AMOUNT NUMBER(20) NULL, VALIDITY NUMBER(10) NULL, BONUS_VALIDITY NUMBER(10) NULL, TRANSFER_CATEGORY VARCHAR2(20 BYTE) DEFAULT 'PRE-PRE' NOT NULL, CONSTRAINT PK_MON_TRANS_SUMM PRIMARY KEY (TRANS_MONTH_YEAR, TRANS_MONTH, SENDER_NETWORK_CODE, RECEIVER_NETWORK_CODE, SERVICE, SUB_SERVICE, TRANSFER_CATEGORY, SENDER_SERVICE_CLASS, RECEIVER_SERVICE_CLASS) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 64K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.MONTHLY_TRANSACTION_SUMMARY.BONUS_AMOUNT IS 'bonus amount given'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.MONTHLY_TRANSACTION_SUMMARY.VALIDITY IS 'validity given  in days '
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.MONTHLY_TRANSACTION_SUMMARY.BONUS_VALIDITY IS 'bonus validity given  in days '
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.MONTHLY_TRANSACTION_SUMMARY.TRANSFER_CATEGORY IS 'Defines the type of service such as Pre to Post, Pre to Pre, Post to Pre'
/


