CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.VAS_CIRCLE_ITEM_PRICES ( VAS_ITEM_ID VARCHAR2(10 BYTE) NOT NULL, VAS_CIRCLE_CODE VARCHAR2(10 BYTE) NOT NULL, VAS_ITEM_PRICE NUMBER(10,4) NOT NULL, STATUS VARCHAR2(1 BYTE) NULL, CREATED_ON DATE NULL, CREATED_BY VARCHAR2(20 BYTE) NULL, MODIFIED_ON DATE NULL, MODIFIED_BY VARCHAR2(20 BYTE) NULL, CONSTRAINT PK_VAS_CIRCLE_ITEM_PRICE PRIMARY KEY (VAS_ITEM_ID, VAS_CIRCLE_CODE) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 64K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ), CONSTRAINT FK_VAS_CIRCLE_ITEM_PRICE FOREIGN KEY (VAS_ITEM_ID) REFERENCES {{ PRETUPS_SCHEMA_USER_NAME }}.VAS_ITEMS (VAS_ITEM_ID), CONSTRAINT FK_VAS_CIRCLE_CODE FOREIGN KEY (VAS_CIRCLE_CODE) REFERENCES {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORKS (NETWORK_CODE) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 64K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
