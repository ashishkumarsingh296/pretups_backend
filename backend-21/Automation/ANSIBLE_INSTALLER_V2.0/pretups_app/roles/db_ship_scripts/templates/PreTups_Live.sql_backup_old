--
-- USER_PHONES  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.USER_PHONES ( USER_PHONES_ID VARCHAR2(10 BYTE) NOT NULL, MSISDN VARCHAR2(15 BYTE) NOT NULL, USER_ID VARCHAR2(15 BYTE) NOT NULL, DESCRIPTION VARCHAR2(20 BYTE) NULL, PRIMARY_NUMBER VARCHAR2(1 BYTE) DEFAULT 'N' NULL, SMS_PIN VARCHAR2(100 BYTE) NULL, PIN_REQUIRED VARCHAR2(1 BYTE) DEFAULT 'Y' NULL, PHONE_PROFILE VARCHAR2(10 BYTE) DEFAULT '1' NULL, PHONE_LANGUAGE VARCHAR2(5 BYTE) DEFAULT 'en' NULL, COUNTRY VARCHAR2(5 BYTE) DEFAULT 'US' NULL, INVALID_PIN_COUNT NUMBER(3) DEFAULT 0 NOT NULL, LAST_TRANSACTION_STATUS VARCHAR2(20 BYTE) DEFAULT 0 NULL, LAST_TRANSACTION_ON DATE NULL, PIN_MODIFIED_ON DATE NOT NULL, CREATED_BY VARCHAR2(20 BYTE) NULL, CREATED_ON DATE NOT NULL, MODIFIED_BY VARCHAR2(20 BYTE) NOT NULL, MODIFIED_ON DATE NOT NULL, LAST_TRANSFER_ID VARCHAR2(20 BYTE) NULL, LAST_TRANSFER_TYPE VARCHAR2(20 BYTE) NULL, PREFIX_ID NUMBER NOT NULL, TEMP_TRANSFER_ID VARCHAR2(10 BYTE) DEFAULT 1111111 NULL, FIRST_INVALID_PIN_TIME DATE NULL, ACCESS_TYPE VARCHAR2(15 BYTE) DEFAULT 'PHONE' NOT NULL, FROM_TIME VARCHAR2(10 BYTE) NULL, TO_TIME VARCHAR2(10 BYTE) NULL, ALLOWED_DAYS VARCHAR2(15 BYTE) NULL, ALLOWED_IP VARCHAR2(150 BYTE) NULL, LAST_LOGIN_ON DATE NULL, PIN_RESET VARCHAR2(1 BYTE) DEFAULT 'N' NULL, LAST_ACCESS_ON DATE NULL, CONSTRAINT PK_USER_PHONES PRIMARY KEY (USER_PHONES_ID) USING INDEX TABLESPACE {{ P2P_INDX_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 320K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 3M NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.USER_PHONES.USER_PHONES_ID IS 'User Phone ID'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.USER_PHONES.MSISDN IS 'Mobile No.'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.USER_PHONES.USER_ID IS 'User ID with phone is asociated'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.USER_PHONES.DESCRIPTION IS 'Description'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.USER_PHONES.PRIMARY_NUMBER IS 'This flag indiacates that whether Phone is Primary or Not'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.USER_PHONES.SMS_PIN IS 'SMS PIN to be used with Phone'
/

