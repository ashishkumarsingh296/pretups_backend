--
-- SUBSCRIBER_CONTROL  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_CONTROL ( MODULE VARCHAR2(10 BYTE) NOT NULL, MSISDN VARCHAR2(15 BYTE) NOT NULL, SUBSCRIBER_TYPE VARCHAR2(10 BYTE) NULL, FAILED_COUNT NUMBER(16) DEFAULT 0 NULL, CONSECUTIVE_FAILURES NUMBER(16) DEFAULT 0 NULL, LAST_FAILURE_ON DATE NULL, SUCCESS_COUNT NUMBER(16) DEFAULT 0 NULL, LAST_SUCCESS_ON DATE NULL, TOTAL_TRANSFER_AMOUNT NUMBER(20) DEFAULT 0 NULL, LAST_TRANSFER_ID VARCHAR2(20 BYTE) NULL, LAST_TRANSFER_STATUS VARCHAR2(1 BYTE) NULL, LAST_TRANSFER_STAGE VARCHAR2(10 BYTE) NULL, LAST_TRANSFER_ON DATE NULL, CREATED_DATE DATE NULL, PREFIX_ID NUMBER(6) NOT NULL, PREV_DAY_SUCCESS_COUNTS NUMBER(20) DEFAULT 0 NULL, PREV_DAY_TRANSFER_AMOUNT NUMBER(20) DEFAULT 0 NULL, WEEKLY_TRANSFER_AMOUNT NUMBER(20) DEFAULT 0 NULL, WEEKLY_SUCCESS_COUNTS NUMBER(20) DEFAULT 0 NULL, PREV_WEEK_SUCCESS_COUNTS NUMBER(20) DEFAULT 0 NULL, PREV_WEEK_TRANSFER_AMOUNT NUMBER(20) DEFAULT 0 NULL, MONTHLY_SUCCESS_COUNTS NUMBER(20) DEFAULT 0 NULL, MONTHLY_TRANSFER_AMOUNT NUMBER(20) DEFAULT 0 NULL, PREV_MONTH_SUCCESS_COUNTS NUMBER(20) DEFAULT 0 NULL, PREV_MONTH_TRANSFER_AMOUNT NUMBER(20) DEFAULT 0 NULL, CONSTRAINT PK_SUBSCRIBER_CONTRL PRIMARY KEY (MSISDN, CREATED_DATE, MODULE) USING INDEX TABLESPACE {{ P2P_INDX_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 18M NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 58M NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_CONTROL.MODULE IS 'Module in which this MSISDN is operating'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_CONTROL.MSISDN IS 'MSISDN of the receiver'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_CONTROL.SUBSCRIBER_TYPE IS 'Subscriber type like POST / PRE'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_CONTROL.FAILED_COUNT IS 'Total failed count for the MSISDN in the created date'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_CONTROL.CONSECUTIVE_FAILURES IS 'Total consecutive fail count for the MSISDN in the created date'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_CONTROL.LAST_FAILURE_ON IS 'Date and time of the last failed tranfer'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_CONTROL.SUCCESS_COUNT IS 'Total Success Transfer till now'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_CONTROL.LAST_SUCCESS_ON IS 'Date and Time of the last success transfer'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_CONTROL.TOTAL_TRANSFER_AMOUNT IS 'Total Success Transfered MSIDN to this MSISDN till now'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_CONTROL.LAST_TRANSFER_ID IS 'Transfer ID of the last transfer to this MSISDN'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_CONTROL.LAST_TRANSFER_STATUS IS 'Status of the last transfer to this MSISDN'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_CONTROL.LAST_TRANSFER_STAGE IS 'Stage of the last transfer to this MSISDN'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_CONTROL.LAST_TRANSFER_ON IS 'Date and Time of the last transfer to this MSISDN'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_CONTROL.CREATED_DATE IS 'Created date, records will be kept for each date for control'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SUBSCRIBER_CONTROL.PREFIX_ID IS 'Prefix ID of the receiver MSISDN'
/


