--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.PAGES ( PAGE_CODE VARCHAR2(10 BYTE) NOT NULL, MODULE_CODE VARCHAR2(10 BYTE) NOT NULL, PAGE_URL VARCHAR2(100 BYTE) NOT NULL, MENU_NAME VARCHAR2(30 BYTE) NOT NULL, MENU_ITEM VARCHAR2(1 BYTE) NOT NULL, SEQUENCE_NO NUMBER NOT NULL, MENU_LEVEL VARCHAR2(1 BYTE) NOT NULL, APPLICATION_ID VARCHAR2(10 BYTE) DEFAULT 1 NOT NULL, CONSTRAINT PK_PAGES PRIMARY KEY (PAGE_CODE) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 256K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

