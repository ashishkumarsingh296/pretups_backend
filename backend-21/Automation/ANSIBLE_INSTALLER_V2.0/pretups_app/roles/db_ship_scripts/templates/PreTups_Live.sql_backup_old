-- USERS  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.USERS ( USER_ID VARCHAR2(20 BYTE) NOT NULL, USER_NAME NVARCHAR2(80) NOT NULL, NETWORK_CODE VARCHAR2(2 BYTE) NOT NULL, LOGIN_ID VARCHAR2(20 BYTE) NULL, PASSWORD VARCHAR2(100 BYTE) NULL, CATEGORY_CODE VARCHAR2(10 BYTE) NOT NULL, PARENT_ID VARCHAR2(15 BYTE) NOT NULL, OWNER_ID VARCHAR2(15 BYTE) NOT NULL, ALLOWED_IP VARCHAR2(150 BYTE) NULL, ALLOWED_DAYS VARCHAR2(15 BYTE) NULL, FROM_TIME VARCHAR2(10 BYTE) NULL, TO_TIME VARCHAR2(10 BYTE) NULL, LAST_LOGIN_ON DATE NULL, EMPLOYEE_CODE VARCHAR2(12 BYTE) NULL, STATUS VARCHAR2(2 BYTE) NULL, EMAIL VARCHAR2(60 BYTE) NULL, PSWD_MODIFIED_ON DATE NULL, CONTACT_PERSON NVARCHAR2(80) NULL, CONTACT_NO VARCHAR2(50 BYTE) NULL, DESIGNATION NVARCHAR2(30) NULL, DIVISION VARCHAR2(10 BYTE) NULL, DEPARTMENT VARCHAR2(10 BYTE) NULL, MSISDN VARCHAR2(15 BYTE) NULL, USER_TYPE VARCHAR2(10 BYTE) NULL, CREATED_BY VARCHAR2(20 BYTE) NOT NULL, CREATED_ON DATE NOT NULL, MODIFIED_BY VARCHAR2(20 BYTE) NOT NULL, MODIFIED_ON DATE NOT NULL, ADDRESS1 NVARCHAR2(50) NULL, ADDRESS2 NVARCHAR2(50) NULL, CITY NVARCHAR2(30) NULL, STATE NVARCHAR2(30) NULL, COUNTRY NVARCHAR2(20) NULL, SSN VARCHAR2(15 BYTE) NULL, USER_NAME_PREFIX NVARCHAR2(10) NULL, EXTERNAL_CODE VARCHAR2(20 BYTE) NULL, USER_CODE VARCHAR2(15 BYTE) NULL, SHORT_NAME NVARCHAR2(15) NULL, REFERENCE_ID VARCHAR2(15 BYTE) NULL, INVALID_PASSWORD_COUNT NUMBER(5) NULL, LEVEL1_APPROVED_BY VARCHAR2(20 BYTE) NULL, LEVEL1_APPROVED_ON DATE NULL, LEVEL2_APPROVED_BY VARCHAR2(20 BYTE) NULL, LEVEL2_APPROVED_ON DATE NULL, APPOINTMENT_DATE DATE NULL, PASSWORD_COUNT_UPDATED_ON DATE DEFAULT SYSDATE NULL, PREVIOUS_STATUS VARCHAR2(2 BYTE) NULL, BATCH_ID VARCHAR2(20 BYTE) NULL, CREATION_TYPE VARCHAR2(1 BYTE) DEFAULT 'M' NULL, REMARKS NVARCHAR2(100) NULL, PSWD_RESET VARCHAR2(1 BYTE) DEFAULT 'N' NULL, CONSTRAINT PK_USERS PRIMARY KEY (USER_ID) USING INDEX TABLESPACE {{ P2P_INDX_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 512K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ), CONSTRAINT UK_USERS_LOGIN_ID UNIQUE (LOGIN_ID) USING INDEX TABLESPACE {{ P2P_INDX_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 384K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 5M NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

