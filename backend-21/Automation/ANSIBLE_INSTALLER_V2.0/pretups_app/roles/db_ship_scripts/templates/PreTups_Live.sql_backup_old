--
-- GROUP_TYPE_PROFILES  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.GROUP_TYPE_PROFILES ( NETWORK_CODE VARCHAR2(2 BYTE) NOT NULL, GROUP_TYPE VARCHAR2(10 BYTE) NOT NULL, REQ_GATEWAY_TYPE VARCHAR2(10 BYTE) NOT NULL, RES_GATEWAY_TYPE VARCHAR2(10 BYTE) NOT NULL, TYPE VARCHAR2(10 BYTE) NOT NULL, THRESHOLD_VALUE NUMBER NOT NULL, GATEWAY_CODE VARCHAR2(10 BYTE) NOT NULL, ALT_GATEWAY_CODE VARCHAR2(10 BYTE) NULL, FREQUENCY VARCHAR2(1 BYTE) DEFAULT 'D' NOT NULL, CONSTRAINT PK_GROUP_TYPE_PROFILES PRIMARY KEY (NETWORK_CODE, GROUP_TYPE, REQ_GATEWAY_TYPE, RES_GATEWAY_TYPE, TYPE) USING INDEX TABLESPACE {{ PRTPINDX_1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 64K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 64K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.GROUP_TYPE_PROFILES.NETWORK_CODE IS 'Code of the network'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.GROUP_TYPE_PROFILES.GROUP_TYPE IS 'Type of group that is associated with service'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.GROUP_TYPE_PROFILES.REQ_GATEWAY_TYPE IS 'Request gateway type'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.GROUP_TYPE_PROFILES.RES_GATEWAY_TYPE IS 'Response gateway type'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.GROUP_TYPE_PROFILES.TYPE IS 'Type of group type(possible values are CHRG or CTRL) CHRG means charging and CTRL means controlling'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.GROUP_TYPE_PROFILES.THRESHOLD_VALUE IS 'Threshold value that is checked with current counters'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.GROUP_TYPE_PROFILES.GATEWAY_CODE IS 'Gateway code to be used to send response in case thresholds are reached'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.GROUP_TYPE_PROFILES.ALT_GATEWAY_CODE IS 'Alternate gateway code to be used to send message , If fail to send response using gateway_code'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.GROUP_TYPE_PROFILES.FREQUENCY IS 'Period of check (possible values are D or M) D means daily and M means monthly'
/


