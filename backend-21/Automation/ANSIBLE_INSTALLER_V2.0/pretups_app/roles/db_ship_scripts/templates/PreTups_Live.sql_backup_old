CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.SERVICE_KEYWORDS ( KEYWORD NVARCHAR2(20) NOT NULL, REQ_INTERFACE_TYPE VARCHAR2(10 BYTE) NOT NULL, SERVICE_PORT VARCHAR2(50 BYTE) NOT NULL, SERVICE_TYPE VARCHAR2(10 BYTE) NOT NULL, NAME NVARCHAR2(30) NULL, STATUS VARCHAR2(1 BYTE) DEFAULT 'Y' NULL, MENU VARCHAR2(30 BYTE) NULL, SUB_MENU VARCHAR2(30 BYTE) NULL, ALLOWED_VERSION VARCHAR2(50 BYTE) NULL, MODIFY_ALLOWED VARCHAR2(1 BYTE) DEFAULT 'Y' NULL, CREATED_ON DATE NOT NULL, CREATED_BY VARCHAR2(20 BYTE) NOT NULL, MODIFIED_ON DATE NOT NULL, MODIFIED_BY VARCHAR2(20 BYTE) NOT NULL, SERVICE_KEYWORD_ID VARCHAR2(10 BYTE) NOT NULL, SUB_KEYWORD NVARCHAR2(10) NULL, CONSTRAINT PK_SERVICE_KEYWORDS PRIMARY KEY (SERVICE_KEYWORD_ID) USING INDEX TABLESPACE {{ P2P_INDX_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ), CONSTRAINT UK_SERVICE_KEYWORDS UNIQUE (SERVICE_TYPE, REQ_INTERFACE_TYPE, KEYWORD, SERVICE_PORT, SUB_KEYWORD) USING INDEX TABLESPACE {{ P2P_INDX_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 64K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ), FOREIGN KEY (SERVICE_TYPE) REFERENCES {{ PRETUPS_SCHEMA_USER_NAME }}.SERVICE_TYPE (SERVICE_TYPE) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

