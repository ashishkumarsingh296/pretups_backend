CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.C2S_TRANSFERS ( TRANSFER_ID VARCHAR2(20 BYTE) NOT NULL, TRANSFER_DATE DATE NOT NULL, TRANSFER_DATE_TIME DATE NOT NULL, NETWORK_CODE VARCHAR2(2 BYTE) NOT NULL, SENDER_ID VARCHAR2(15 BYTE) NULL, SENDER_CATEGORY VARCHAR2(10 BYTE) NULL, PRODUCT_CODE VARCHAR2(10 BYTE) NULL, SENDER_MSISDN VARCHAR2(15 BYTE) NULL, RECEIVER_MSISDN VARCHAR2(15 BYTE) NULL, RECEIVER_NETWORK_CODE VARCHAR2(2 BYTE) NULL, TRANSFER_VALUE NUMBER(20) NULL, ERROR_CODE VARCHAR2(40 BYTE) NULL, REQUEST_GATEWAY_TYPE VARCHAR2(10 BYTE) NULL, REQUEST_GATEWAY_CODE VARCHAR2(10 BYTE) NULL, REFERENCE_ID VARCHAR2(50 BYTE) NULL, SERVICE_TYPE VARCHAR2(10 BYTE) NULL, DIFFERENTIAL_APPLICABLE VARCHAR2(1 BYTE) DEFAULT 'N' NULL, PIN_SENT_TO_MSISDN VARCHAR2(15 BYTE) NULL, LANGUAGE VARCHAR2(10 BYTE) NULL, COUNTRY VARCHAR2(20 BYTE) NULL, SKEY NUMBER(10) NULL, SKEY_GENERATION_TIME DATE NULL, SKEY_SENT_TO_MSISDN VARCHAR2(15 BYTE) NULL, REQUEST_THROUQUEUE VARCHAR2(1 BYTE) NULL, CREDIT_BACK_STATUS VARCHAR2(5 BYTE) NULL, QUANTITY NUMBER(20) NULL, RECONCILIATION_FLAG VARCHAR2(1 BYTE) NULL, RECONCILIATION_DATE DATE NULL, RECONCILIATION_BY VARCHAR2(30 BYTE) NULL, CREATED_ON DATE NOT NULL, CREATED_BY VARCHAR2(20 BYTE) NOT NULL, MODIFIED_ON DATE NOT NULL, MODIFIED_BY VARCHAR2(20 BYTE) NOT NULL, TRANSFER_STATUS VARCHAR2(5 BYTE) NULL, CARD_GROUP_SET_ID VARCHAR2(5 BYTE) NULL, VERSION VARCHAR2(5 BYTE) NULL, CARD_GROUP_ID VARCHAR2(10 BYTE) NULL, SENDER_TRANSFER_VALUE NUMBER(20) NULL, RECEIVER_ACCESS_FEE NUMBER(20) NULL, RECEIVER_TAX1_TYPE VARCHAR2(3 BYTE) NULL, RECEIVER_TAX1_RATE NUMBER(20,4) NULL, RECEIVER_TAX1_VALUE NUMBER(20) NULL, RECEIVER_TAX2_TYPE VARCHAR2(3 BYTE) NULL, RECEIVER_TAX2_RATE NUMBER(20,4) NULL, RECEIVER_TAX2_VALUE NUMBER(20) NULL, RECEIVER_VALIDITY NUMBER(5) NULL, RECEIVER_TRANSFER_VALUE NUMBER(20) NULL, RECEIVER_BONUS_VALUE NUMBER(20) NULL, RECEIVER_GRACE_PERIOD NUMBER(5) NULL, RECEIVER_BONUS_VALIDITY NUMBER(5) NULL, CARD_GROUP_CODE VARCHAR2(10 BYTE) NULL, RECEIVER_VALPERIOD_