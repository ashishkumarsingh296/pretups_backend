:NEW.stock_created,:NEW.stock_returned,:NEW.stock,:NEW.stock_sold,
:NEW.last_txn_no,:NEW.last_txn_type,:NEW.last_txn_stock,:OLD.stock,
:NEW.modified_by,:NEW.modified_on,:NEW.created_on,:NEW.created_by,sysdate,'U',:NEW.daily_stock_updated_on);
