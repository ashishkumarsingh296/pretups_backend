--
-- CHANNEL_USERS  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.CHANNEL_USERS ( USER_ID VARCHAR2(15 BYTE) NOT NULL, USER_GRADE VARCHAR2(10 BYTE) NULL, CONTACT_PERSON NVARCHAR2(80) NULL, TRANSFER_PROFILE_ID VARCHAR2(10 BYTE) NULL, COMM_PROFILE_SET_ID VARCHAR2(10 BYTE) NULL, IN_SUSPEND VARCHAR2(1 BYTE) DEFAULT 'N' NULL, OUT_SUSPEND VARCHAR2(1 BYTE) DEFAULT 'N' NULL, OUTLET_CODE VARCHAR2(10 BYTE) NULL, SUBOUTLET_CODE VARCHAR2(10 BYTE) NULL, ACTIVATED_ON DATE NULL, APPLICATION_ID VARCHAR2(20 BYTE) DEFAULT '2' NOT NULL, MPAY_PROFILE_ID VARCHAR2(20 BYTE) NULL, USER_PROFILE_ID VARCHAR2(20 BYTE) NOT NULL, IS_PRIMARY VARCHAR2(1 BYTE) DEFAULT 'Y' NOT NULL, MCOMMERCE_SERVICE_ALLOW VARCHAR2(1 BYTE) DEFAULT 'N' NOT NULL, LOW_BAL_ALERT_ALLOW VARCHAR2(1 BYTE) DEFAULT 'N' NOT NULL, MCATEGORY_CODE VARCHAR2(10 BYTE) NULL, PRIMARY KEY (USER_ID) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 512K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ), UNIQUE (USER_PROFILE_ID) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 512K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 2M NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.CHANNEL_USERS.LOW_BAL_ALERT_ALLOW IS 'Low balance alert allow for this users.'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.CHANNEL_USERS.USER_ID IS 'User ID of User from Users table'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.CHANNEL_USERS.USER_GRADE IS 'Grade Associated with User'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.CHANNEL_USERS.CONTACT_PERSON IS 'Contact Person'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.CHANNEL_USERS.TRANSFER_PROFILE_ID IS 'ID of Transfer Profile associated with User'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.CHANNEL_USERS.COMM_PROFILE_SET_ID IS 'ID of Commission Transfer Profile associated with User'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.CHANNEL_USERS.IN_SUSPEND IS 'This field indicates whether User''s In Transactions are suspended or not.i.e whether user can purchase talk value.'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.CHANNEL_USERS.OUT_SUSPEND IS 'This field indicates whether User''s Out Transactions are suspended or not.i.e whether user can sell talk value.'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.CHANNEL_USERS.ACTIVATED_ON IS 'This value defines when the channel user is activated'
/


