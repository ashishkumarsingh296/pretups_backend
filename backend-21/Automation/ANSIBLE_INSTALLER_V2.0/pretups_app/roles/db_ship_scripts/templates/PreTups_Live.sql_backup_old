--
-- P2P_SUBSCRIBERS  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_SUBSCRIBERS ( USER_ID VARCHAR2(20 BYTE) NOT NULL, MSISDN VARCHAR2(15 BYTE) NOT NULL, SUBSCRIBER_TYPE VARCHAR2(10 BYTE) NOT NULL, USER_NAME NVARCHAR2(50) NULL, STATUS VARCHAR2(1 BYTE) NULL, NETWORK_CODE VARCHAR2(2 BYTE) NULL, PIN VARCHAR2(100 BYTE) NULL, PIN_BLOCK_COUNT NUMBER(3) NULL, LAST_TRANSFER_AMOUNT NUMBER(20) DEFAULT 0 NULL, LAST_TRANSFER_ON DATE NULL, LAST_TRANSFER_TYPE VARCHAR2(15 BYTE) NULL, LAST_TRANSFER_STATUS VARCHAR2(10 BYTE) NULL, BUDDY_SEQ_NUMBER NUMBER(3) NULL, TOTAL_TRANSFERS NUMBER(16) DEFAULT 0 NULL, TOTAL_TRANSFER_AMOUNT NUMBER(20) DEFAULT 0 NULL, REQUEST_STATUS VARCHAR2(1 BYTE) NULL, BILLING_TYPE VARCHAR2(10 BYTE) NULL, BILLING_CYCLE_DATE DATE NULL, CREDIT_LIMIT NUMBER(20) NULL, ACTIVATED_ON DATE NULL, REGISTERED_ON DATE NULL, CREATED_ON DATE NOT NULL, CREATED_BY VARCHAR2(20 BYTE) NOT NULL, MODIFIED_ON DATE NOT NULL, MODIFIED_BY VARCHAR2(20 BYTE) NOT NULL, LAST_TRANSFER_ID VARCHAR2(20 BYTE) NULL, CONSECUTIVE_FAILURES NUMBER(16) DEFAULT 0 NULL, LAST_TRANSFER_MSISDN VARCHAR2(15 BYTE) NULL, SKEY_REQUIRED VARCHAR2(1 BYTE) NULL, DAILY_TRANSFER_COUNT NUMBER(16) DEFAULT 0 NULL, MONTHLY_TRANSFER_COUNT NUMBER(16) DEFAULT 0 NULL, WEEKLY_TRANSFER_COUNT NUMBER(16) DEFAULT 0 NULL, PREV_DAILY_TRANSFER_COUNT NUMBER(16) DEFAULT 0 NULL, PREV_WEEKLY_TRANSFER_COUNT NUMBER(16) DEFAULT 0 NULL, PREV_MONTHLY_TRANSFER_COUNT NUMBER(16) DEFAULT 0 NULL, SERVICE_CLASS_CODE VARCHAR2(20 BYTE) NULL, DAILY_TRANSFER_AMOUNT NUMBER(20) DEFAULT 0 NULL, WEEKLY_TRANSFER_AMOUNT NUMBER(20) DEFAULT 0 NULL, MONTHLY_TRANSFER_AMOUNT NUMBER(20) DEFAULT 0 NULL, PREV_DAILY_TRANSFER_AMOUNT NUMBER(20) DEFAULT 0 NULL, PREV_WEEKLY_TRANSFER_AMOUNT NUMBER(20) DEFAULT 0 NULL, PREV_MONTHLY_TRANSFER_AMOUNT NUMBER(20) DEFAULT 0 NULL, PREV_TRANSFER_DATE DATE NULL, PREV_TRANSFER_WEEK_DATE DATE NULL, PREV_TRANSFER_MONTH_DATE DATE NULL, LAST_SUCCESS_TRANSFER_DATE DATE NULL, PREFIX_ID NUMBER(6) NOT NULL, PIN_MODIFIED_ON DATE NULL, FIRST_INVALID_PIN_TIME DATE NULL, LANGUAGE VARCHAR2(5 BYTE) NULL, COUNTRY VARCHAR2(5 BYTE) NULL, SERVICE_CLASS_ID VARCHAR2(10 BYTE) NULL, LOGIN_ID VARCHAR2(20 BYTE) NULL, PASSWORD VARCHAR2(100 BYTE) NULL, INVALID_PASSWORD_COUNT NUMBER(1) NULL, LAST_LOGIN_ON DATE NULL, PSWD_MODIFIED_ON DATE NULL, PASSWORD_COUNT_UPDATED_ON DATE NULL, PSWD_RESET VARCHAR2(1 BYTE) DEFAULT 'N' NULL, PRIMARY KEY (USER_ID) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 4M NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 28M NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_SUBSCRIBERS.MSISDN IS 'Mobile no from which the user has registered'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_SUBSCRIBERS.SUBSCRIBER_TYPE IS 'Type of Subscriber Prepaid or Postpaid'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_SUBSCRIBERS.USER_NAME IS 'Name of the user if available'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_SUBSCRIBERS.STATUS IS 'Status of the User like Active etc'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_SUBSCRIBERS.NETWORK_CODE IS 'Network to which the user belongs'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_SUBSCRIBERS.PIN IS 'PIN used by the user to access the account'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_SUBSCRIBERS.PIN_BLOCK_COUNT IS 'Total consecutive inavlid PIN request counter used for blocking the user'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_SUBSCRIBERS.LAST_TRANSFER_AMOUNT IS 'Amount that was last transferred by the user'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_SUBSCRIBERS.LAST_TRANSFER_ON IS 'Date time of the last transfer'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_SUBSCRIBERS.LAST_TRANSFER_TYPE IS 'Type of the last transfer done by the user, this is service type requested'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_SUBSCRIBERS.LAST_TRANSFER_STATUS IS 'Status of the last transfer by the user'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_SUBSCRIBERS.BUDDY_SEQ_NUMBER IS 'Total Buddies added by the user'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_SUBSCRIBERS.TOTAL_TRANSFERS IS 'Total transfers done so far by the user'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_SUBSCRIBERS.TOTAL_TRANSFER_AMOUNT IS 'Total transfer amount done by the user so far'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.P2P_SUBSCRIBERS.REQUEST_STATUS IS 'Request status used to mark the underprocess last request'
