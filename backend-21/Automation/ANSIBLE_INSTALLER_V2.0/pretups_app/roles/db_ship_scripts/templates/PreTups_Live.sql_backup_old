--
-- SCHEDULED_BATCH_MASTER_HISTORY  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY ( BATCH_ID VARCHAR2(15 BYTE) NULL, STATUS VARCHAR2(1 BYTE) NOT NULL, NETWORK_CODE VARCHAR2(2 BYTE) NOT NULL, TOTAL_COUNT NUMBER(16) NOT NULL, SUCCESSFUL_COUNT NUMBER(16) NULL, UPLOAD_FAILED_COUNT NUMBER(16) NULL, PROCESS_FAILED_COUNT NUMBER(16) NULL, CANCELLED_COUNT NUMBER(16) NULL, SCHEDULED_DATE DATE NOT NULL, PARENT_ID VARCHAR2(20 BYTE) NULL, OWNER_ID VARCHAR2(20 BYTE) NULL, PARENT_CATEGORY VARCHAR2(10 BYTE) NOT NULL, PARENT_DOMAIN VARCHAR2(10 BYTE) NOT NULL, SERVICE_TYPE VARCHAR2(10 BYTE) NULL, CREATED_ON DATE NOT NULL, CREATED_BY VARCHAR2(20 BYTE) NOT NULL, MODIFIED_ON DATE NOT NULL, MODIFIED_BY VARCHAR2(20 BYTE) NOT NULL, INITIATED_BY VARCHAR2(20 BYTE) NULL, ENTRY_DATE DATE NULL, OPERATION_PERFORMED VARCHAR2(1 BYTE) NULL, REF_BATCH_ID VARCHAR2(15 BYTE) NULL, ACTIVE_USER_ID VARCHAR2(15 BYTE) NULL ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

