/


--
-- TRANSACTION_LOAD  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSACTION_LOAD
(
  INSTANCE_ID             VARCHAR2(5 BYTE)      NOT NULL,
  NETWORK_CODE            VARCHAR2(2 BYTE)      NOT NULL,
  INTERFACE_ID            VARCHAR2(10 BYTE)     NOT NULL,
  SERVICE_TYPE            VARCHAR2(8 BYTE)      NOT NULL,
  MAX_TRANSACTION_LOAD    NUMBER(3),
  MIN_SERVICE_TIMEOUT     NUMBER(7),
  OVER_FLOW_COUNT         NUMBER(2),
  CREATED_ON              DATE                  NOT NULL,
  CREATED_BY              VARCHAR2(20 BYTE)     NOT NULL,
  MODIFIED_ON             DATE                  NOT NULL,
  MODIFIED_BY             VARCHAR2(20 BYTE)     NOT NULL,
  TRANSACTION_TPS         NUMBER(5),
  REQUEST_TIME_OUT        NUMBER(7),
  NEXT_CHECK_TIMEOUT_SEC  NUMBER(7),
  PRIMARY KEY (INSTANCE_ID, NETWORK_CODE, INTERFACE_ID, SERVICE_TYPE)
)
TABLESPACE {{ P2P_INDX_TS_NAME }}
PCTUSED    40
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          160K
            NEXT             16K
            MINEXTENTS       1
            MAXEXTENTS       505
            PCTINCREASE      50
            FREELISTS        1
            FREELIST GROUPS  1
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSACTION_LOAD ( INSTANCE_ID VARCHAR2(5 BYTE) NOT NULL, NETWORK_CODE VARCHAR2(2 BYTE) NOT NULL, INTERFACE_ID VARCHAR2(10 BYTE) NOT NULL, SERVICE_TYPE VARCHAR2(5 BYTE) NOT NULL, MAX_TRANSACTION_LOAD NUMBER(3) NULL, MIN_SERVICE_TIMEOUT NUMBER(7) NULL, OVER_FLOW_COUNT NUMBER(2) NULL, CREATED_ON DATE NOT NULL, CREATED_BY VARCHAR2(20 BYTE) NOT NULL, MODIFIED_ON DATE NOT NULL, MODIFIED_BY VARCHAR2(20 BYTE) NOT NULL, TRANSACTION_TPS NUMBER(5) NULL, REQUEST_TIME_OUT NUMBER(7) NULL, NEXT_CHECK_TIMEOUT_SEC NUMBER(7) NULL, PRIMARY KEY (INSTANCE_ID, NETWORK_CODE, INTERFACE_ID, SERVICE_TYPE) USING INDEX TABLESPACE {{ P2P_INDX_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ), FOREIGN KEY (INSTANCE_ID, NETWORK_CODE) REFERENCES {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_LOAD (INSTANCE_ID,NETWORK_CODE) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSACTION_LOAD.INSTANCE_ID IS 'Instance ID of the Application server'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSACTION_LOAD.NETWORK_CODE IS 'Network Code in which the interface is running'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSACTION_LOAD.INTERFACE_ID IS 'Interface ID in which the services are running like IN is one interface'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSACTION_LOAD.SERVICE_TYPE IS 'Type of service catered by the IN like P2P and C2S'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSACTION_LOAD.MAX_TRANSACTION_LOAD IS 'Load that can levied for this service type in the interface'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSACTION_LOAD.MIN_SERVICE_TIMEOUT IS 'Minimum time required by the transaction to complete'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSACTION_LOAD.OVER_FLOW_COUNT IS 'No of over flow allowed that will used in using the counters of alternate service'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSACTION_LOAD.CREATED_ON IS 'Date time when the entry was created'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSACTION_LOAD.CREATED_BY IS 'User ID of the user who created the entry'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSACTION_LOAD.MODIFIED_ON IS 'Date time when the entry was last modified'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSACTION_LOAD.MODIFIED_BY IS 'User ID of the user who last modifed the entry'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSACTION_LOAD.TRANSACTION_TPS IS 'Transaction Per second allowed for this service in the interface'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSACTION_LOAD.REQUEST_TIME_OUT IS 'Time out in seconds after which the request will get time out'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSACTION_LOAD.NEXT_CHECK_TIMEOUT_SEC IS 'Seconds after when the checks for Time out cases will be carried by the Thread'
/


--
-- PAGE_ROLES  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.PAGE_ROLES ( ROLE_CODE VARCHAR2(30 BYTE) NOT NULL, PAGE_CODE VARCHAR2(10 BYTE) NOT NULL, APPLICATION_ID VARCHAR2(10 BYTE) DEFAULT 1 NOT NULL, CONSTRAINT PK_PAGE_ROLES PRIMARY KEY (ROLE_CODE, PAGE_CODE) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 192K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ), CONSTRAINT UNQ_PAGE_ROLES UNIQUE (PAGE_CODE, ROLE_CODE) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 192K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ), CONSTRAINT FK_PAGECODE_PAGE_ROLES FOREIGN KEY (PAGE_CODE) REFERENCES {{ PRETUPS_SCHEMA_USER_NAME }}.PAGES (PAGE_CODE) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PAGE_ROLES.ROLE_CODE IS 'Role Code with which Page is going to be associated'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PAGE_ROLES.PAGE_CODE IS 'Page Code of Page associated with corresponding Role'
/


