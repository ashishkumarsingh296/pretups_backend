--
-- DOMAINS  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.DOMAINS ( DOMAIN_CODE VARCHAR2(10 BYTE) NOT NULL, DOMAIN_NAME NVARCHAR2(40) NOT NULL, DOMAIN_TYPE_CODE VARCHAR2(10 BYTE) NOT NULL, OWNER_CATEGORY VARCHAR2(10 BYTE) NOT NULL, STATUS VARCHAR2(1 BYTE) NOT NULL, CREATED_ON DATE NOT NULL, CREATED_BY VARCHAR2(20 BYTE) NOT NULL, MODIFIED_ON DATE NOT NULL, MODIFIED_BY VARCHAR2(20 BYTE) NOT NULL, NUM_OF_CATEGORIES NUMBER(3) NOT NULL, APPLICATION_CODE VARCHAR2(10 BYTE) DEFAULT 2 NOT NULL, CAT_PROFILE_ALLOWED VARCHAR2(1 BYTE) NULL, CONSTRAINT PK_DOMAINS PRIMARY KEY (DOMAIN_CODE) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ), CONSTRAINT UK_DOMAINS UNIQUE (DOMAIN_NAME) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

