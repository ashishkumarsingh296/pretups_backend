--
-- TRANSFER_RULES  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSFER_RULES ( MODULE VARCHAR2(10 BYTE) NOT NULL, NETWORK_CODE VARCHAR2(2 BYTE) NOT NULL, SENDER_SUBSCRIBER_TYPE VARCHAR2(20 BYTE) NOT NULL, RECEIVER_SUBSCRIBER_TYPE VARCHAR2(20 BYTE) NOT NULL, SENDER_SERVICE_CLASS_ID VARCHAR2(10 BYTE) NOT NULL, RECEIVER_SERVICE_CLASS_ID VARCHAR2(10 BYTE) NOT NULL, CREATED_ON DATE NOT NULL, CREATED_BY VARCHAR2(20 BYTE) NOT NULL, MODIFIED_ON DATE NOT NULL, MODIFIED_BY VARCHAR2(20 BYTE) NOT NULL, CARD_GROUP_SET_ID VARCHAR2(5 BYTE) NOT NULL, STATUS VARCHAR2(1 BYTE) DEFAULT 'Y' NULL, SUB_SERVICE VARCHAR2(10 BYTE) NULL, RULE_TYPE VARCHAR2(1 BYTE) DEFAULT 'N' NOT NULL, RULE_LEVEL VARCHAR2(3 BYTE) DEFAULT 'NA' NOT NULL, START_TIME DATE NULL, END_TIME DATE NULL, SERVICE_TYPE VARCHAR2(10 BYTE) NOT NULL, DATE_RANGE VARCHAR2(5 BYTE) NULL, TIME_SLAB VARCHAR2(240 BYTE) NULL, CONSTRAINT PK_TRANSFER_RULES PRIMARY KEY (MODULE, NETWORK_CODE, RECEIVER_SERVICE_CLASS_ID, RECEIVER_SUBSCRIBER_TYPE, SENDER_SERVICE_CLASS_ID, SENDER_SUBSCRIBER_TYPE, SUB_SERVICE, SERVICE_TYPE, RULE_LEVEL) USING INDEX TABLESPACE {{ P2P_INDX_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 64K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSFER_RULES.MODULE IS 'module code, P2P or C2S'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSFER_RULES.NETWORK_CODE IS 'network code'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSFER_RULES.SENDER_SUBSCRIBER_TYPE IS 'Sender Subscriber type, Prepaid - PRE, postpaid - POST'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSFER_RULES.RECEIVER_SUBSCRIBER_TYPE IS 'Receiver subscriber type, prepaid - PRE, postpaid - POST'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSFER_RULES.SENDER_SERVICE_CLASS_ID IS 'Sender service class id from service class table'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSFER_RULES.RECEIVER_SERVICE_CLASS_ID IS 'Receiver service class id from service class table'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSFER_RULES.CARD_GROUP_SET_ID IS 'card group set id'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSFER_RULES.STATUS IS 'status of the transfer rule, Active - Y, suspended - S,  Deleted - N'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSFER_RULES.RULE_TYPE IS 'N for normal and P for Promotional transfer rule'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSFER_RULES.RULE_LEVEL IS 'USR(user level), GRD(grade level),CAT(category level),GRP(geography level),NA(for normal transfer rules)'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSFER_RULES.START_TIME IS 'date from when promotional transfer rule is applicable'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSFER_RULES.END_TIME IS 'date upto which promotional transfer rule is applicable'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.TRANSFER_RULES.SERVICE_TYPE IS '''service type for which transfer rule is created'''
/


