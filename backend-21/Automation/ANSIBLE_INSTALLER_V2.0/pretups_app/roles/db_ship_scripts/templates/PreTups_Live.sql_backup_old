-- NETWORK_SERVICES  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_SERVICES ( MODULE_CODE VARCHAR2(10 BYTE) NOT NULL, SERVICE_TYPE VARCHAR2(10 BYTE) NOT NULL, SENDER_NETWORK VARCHAR2(2 BYTE) NULL, RECEIVER_NETWORK VARCHAR2(2 BYTE) NULL, STATUS VARCHAR2(1 BYTE) NOT NULL, LANGUAGE1_MESSAGE VARCHAR2(150 BYTE) NULL, LANGUAGE2_MESSAGE NVARCHAR2(150) NULL, CREATED_BY VARCHAR2(20 BYTE) NOT NULL, CREATED_ON DATE NOT NULL, MODIFIED_BY VARCHAR2(20 BYTE) NOT NULL, MODIFIED_ON DATE NULL, CONSTRAINT PK_NETWORK_SERVICES PRIMARY KEY (MODULE_CODE, SERVICE_TYPE, RECEIVER_NETWORK, SENDER_NETWORK) USING INDEX TABLESPACE {{ P2P_INDX_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_SERVICES.MODULE_CODE IS 'Module Code of Service'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_SERVICES.SERVICE_TYPE IS 'Service Type code from the services table'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_SERVICES.SENDER_NETWORK IS 'Sender network code'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_SERVICES.RECEIVER_NETWORK IS 'receiver network code'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_SERVICES.STATUS IS 'service status , Y - active, S - Suspended'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_SERVICES.LANGUAGE1_MESSAGE IS 'Language 1 message if service is supended'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_SERVICES.LANGUAGE2_MESSAGE IS 'Language 2 message if service is suspended and language 2 is available in system'
/


--
-- NETWORK_PRODUCT_MAPPING  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORK_PRODUCT_MAPPING ( NETWORK_CODE VARCHAR2(2 BYTE) NULL, PRODUCT_CODE VARCHAR2(10 BYTE) NULL, PRODUCT_USAGE VARCHAR2(1 BYTE) NULL, STATUS VARCHAR2(1 BYTE) NULL, LANGUAGE_MESSAGE1 VARCHAR2(150 BYTE) NULL, LANGUAGE_MESSAGE2 NVARCHAR2(150) NULL, CREATED_BY VARCHAR2(20 BYTE) NULL, CREATED_DATE DATE NULL, MODIFIED_ON DATE NULL, MODIFIED_BY VARCHAR2(20 BYTE) NULL, ALERTING_BALANCE LONG DEFAULT 0 NOT NULL, CONSTRAINT PK_NETWORK_PRODUCT_MAP PRIMARY KEY (NETWORK_CODE, PRODUCT_CODE) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ), CONSTRAINT FK_NPM_NETWORK_CODE FOREIGN KEY (NETWORK_CODE) REFERENCES {{ PRETUPS_SCHEMA_USER_NAME }}.NETWORKS (NETWORK_CODE) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
