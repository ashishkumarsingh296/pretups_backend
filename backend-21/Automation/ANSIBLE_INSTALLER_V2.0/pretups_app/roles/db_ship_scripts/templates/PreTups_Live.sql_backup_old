CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.CHNL_TRANSFER_RULES ( TRANSFER_RULE_ID VARCHAR2(10 BYTE) NOT NULL, DOMAIN_CODE VARCHAR2(10 BYTE) NOT NULL, NETWORK_CODE VARCHAR2(2 BYTE) NOT NULL, FROM_CATEGORY VARCHAR2(10 BYTE) NOT NULL, TO_CATEGORY VARCHAR2(10 BYTE) NOT NULL, TRANSFER_CHNL_BYPASS_ALLOWED VARCHAR2(1 BYTE) NULL, WITHDRAW_ALLOWED VARCHAR2(1 BYTE) NULL, WITHDRAW_CHNL_BYPASS_ALLOWED VARCHAR2(1 BYTE) NULL, RETURN_ALLOWED VARCHAR2(1 BYTE) NULL, RETURN_CHNL_BYPASS_ALLOWED VARCHAR2(1 BYTE) NULL, APPROVAL_REQUIRED VARCHAR2(1 BYTE) NULL, FIRST_APPROVAL_LIMIT NUMBER(20) NULL, SECOND_APPROVAL_LIMIT NUMBER(20) NULL, CREATED_BY VARCHAR2(20 BYTE) NOT NULL, CREATED_ON DATE NOT NULL, MODIFIED_BY VARCHAR2(20 BYTE) NOT NULL, MODIFIED_ON DATE NOT NULL, STATUS VARCHAR2(1 BYTE) DEFAULT 'Y' NOT NULL, TYPE VARCHAR2(10 BYTE) NULL, UNCNTRL_TRANSFER_ALLOWED VARCHAR2(1 BYTE) DEFAULT 'N' NULL, TRANSFER_TYPE VARCHAR2(25 BYTE) NULL, PARENT_ASSOCIATION_ALLOWED VARCHAR2(1 BYTE) NULL, DIRECT_TRANSFER_ALLOWED VARCHAR2(1 BYTE) NULL, TRANSFER_ALLOWED VARCHAR2(1 BYTE) NULL, FOC_TRANSFER_TYPE VARCHAR2(25 BYTE) NULL, FOC_ALLOWED VARCHAR2(1 BYTE) NULL, RESTRICTED_MSISDN_ACCESS VARCHAR2(1 BYTE) DEFAULT 'N' NULL, TO_DOMAIN_CODE VARCHAR2(10 BYTE) NOT NULL, UNCNTRL_TRANSFER_LEVEL VARCHAR2(20 BYTE) NOT NULL, CNTRL_TRANSFER_LEVEL VARCHAR2(20 BYTE) NOT NULL, FIXED_TRANSFER_LEVEL VARCHAR2(20 BYTE) NOT NULL, FIXED_TRANSFER_CATEGORY VARCHAR2(50 BYTE) NULL, UNCNTRL_RETURN_ALLOWED VARCHAR2(1 BYTE) NOT NULL, UNCNTRL_RETURN_LEVEL VARCHAR2(20 BYTE) NOT NULL, CNTRL_RETURN_LEVEL VARCHAR2(20 BYTE) NOT NULL, FIXED_RETURN_LEVEL VARCHAR2(20 BYTE) NOT NULL, FIXED_RETURN_CATEGORY VARCHAR2(50 BYTE) NULL, UNCNTRL_WITHDRAW_ALLOWED VARCHAR2(1 BYTE) NOT NULL, UNCNTRL_WITHDRAW_LEVEL VARCHAR2(20 BYTE) NOT NULL, CNTRL_WITHDRAW_LEVEL VARCHAR2(20 BYTE) NOT NULL, FIXED_WITHDRAW_LEVEL VARCHAR2(20 BYTE) NOT NULL, FIXED_WITHDRAW_CATEGORY VARCHAR2(50 BYTE) NULL, PARENT_ASSOCATION_ALLOWED VARCHAR2(1 BYTE) NULL, RESTRICTED_RECHARGE_ALLOWED VARCHAR2(1 BYTE) DEFAULT 'N' NULL, DIRECT_PAYOUT_ALLOWED VARCHAR2(1 BYTE) DEFAULT 'N' NULL, CONSTRAINT PK_CHNL_TRANSFER_RULES PRIMARY KEY (TRANSFER_RULE_ID) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 64K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS CACHE NOPARALLEL MONITORING
/

