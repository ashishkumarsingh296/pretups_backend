/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.STATUS IS 'Scheduled (S), Executed (E), Cancelled (C)'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.NETWORK_CODE IS 'Network code of the subscriber'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.TOTAL_COUNT IS 'Total number of topups to be done'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.SUCCESSFUL_COUNT IS 'Total number of successful topups'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.UPLOAD_FAILED_COUNT IS 'Number of records for which uploading is failed'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.PROCESS_FAILED_COUNT IS 'Number of records for which topup is failed'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.CANCELLED_COUNT IS 'Number of records for which topup is cancelled before schedule was executed'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.SCHEDULED_DATE IS 'Date on which batch is to be executed'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.PARENT_ID IS 'ID of the parent of the batch'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.OWNER_ID IS 'ID of the owner of the batch'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.PARENT_CATEGORY IS 'Category of parent'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.PARENT_DOMAIN IS 'Domain code of parent'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.SERVICE_TYPE IS 'Service type may be prepaid or postpaid'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.CREATED_ON IS 'Date on which record has been created'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.CREATED_BY IS 'User, who entered the record'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.MODIFIED_ON IS 'Date on which record has been modified last time'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.MODIFIED_BY IS 'User, who last modified the record'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.INITIATED_BY IS 'ID of the channel user who initiated the batch, could be anyone in the hierarchy'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.ENTRY_DATE IS 'Date on which entry has been done in history table'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER_HISTORY.OPERATION_PERFORMED IS 'Operation that made the entry: Update (U)'
/


--
-- SCHEDULED_BATCH_MASTER  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER ( BATCH_ID VARCHAR2(15 BYTE) NULL, STATUS VARCHAR2(1 BYTE) NOT NULL, NETWORK_CODE VARCHAR2(2 BYTE) NOT NULL, TOTAL_COUNT NUMBER(16) NOT NULL, SUCCESSFUL_COUNT NUMBER(16) NULL, UPLOAD_FAILED_COUNT NUMBER(16) NULL, PROCESS_FAILED_COUNT NUMBER(16) NULL, CANCELLED_COUNT NUMBER(16) NULL, SCHEDULED_DATE DATE NOT NULL, PARENT_ID VARCHAR2(20 BYTE) NULL, OWNER_ID VARCHAR2(20 BYTE) NULL, PARENT_CATEGORY VARCHAR2(10 BYTE) NOT NULL, PARENT_DOMAIN VARCHAR2(10 BYTE) NOT NULL, SERVICE_TYPE VARCHAR2(10 BYTE) NULL, CREATED_ON DATE NOT NULL, CREATED_BY VARCHAR2(20 BYTE) NOT NULL, MODIFIED_ON DATE NOT NULL, MODIFIED_BY VARCHAR2(20 BYTE) NOT NULL, INITIATED_BY VARCHAR2(20 BYTE) NULL, REF_BATCH_ID VARCHAR2(15 BYTE) NULL, BATCH_TYPE VARCHAR2(20 BYTE) DEFAULT 'CORPORATE' NOT NULL, ACTIVE_USER_ID VARCHAR2(15 BYTE) NULL, CONSTRAINT PK_SCHEDULED_TRANS PRIMARY KEY (BATCH_ID) USING INDEX TABLESPACE {{ P2P_INDX_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

COMMENT ON TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.SCHEDULED_BATCH_MASTER IS 'batch_id status network_code total_count successful_count upload_failed_count process_failed_count cancelled_count scheduled_date parent_id owner_id  
parent_category parent_domain service_type created_on created_by modified_on modified_by initiated_by'
/

