-- PRODUCTS  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.PRODUCTS ( PRODUCT_CODE VARCHAR2(10 BYTE) NOT NULL, PRODUCT_TYPE VARCHAR2(10 BYTE) NOT NULL, MODULE_CODE VARCHAR2(20 BYTE) NOT NULL, PRODUCT_NAME NVARCHAR2(30) NOT NULL, SHORT_NAME NVARCHAR2(15) NOT NULL, PRODUCT_SHORT_CODE NUMBER(5) NOT NULL, PRODUCT_CATEGORY VARCHAR2(20 BYTE) NOT NULL, ERP_PRODUCT_CODE VARCHAR2(20 BYTE) NOT NULL, STATUS VARCHAR2(1 BYTE) NOT NULL, UNIT_VALUE NUMBER(20) NOT NULL, CREATED_ON DATE NOT NULL, CREATED_BY VARCHAR2(20 BYTE) NOT NULL, MODIFIED_ON DATE NOT NULL, MODIFIED_BY VARCHAR2(20 BYTE) NOT NULL, CONSTRAINT PK_PRODUCTS PRIMARY KEY (PRODUCT_CODE) USING INDEX TABLESPACE {{ PRTPINDX_1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ), CONSTRAINT UK_PRODUCT_SHORT_CODE UNIQUE (PRODUCT_SHORT_CODE) USING INDEX TABLESPACE {{ PRTPINDX_1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PRODUCTS.PRODUCT_CODE IS 'Product code, unique'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PRODUCTS.PRODUCT_TYPE IS 'product type, PREPAID, POSTPAID, EVD'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PRODUCTS.MODULE_CODE IS 'module code, P2P, C2S'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PRODUCTS.PRODUCT_NAME IS 'Product Name'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PRODUCTS.SHORT_NAME IS 'Product short Name , used in SMS Message, should be single word'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PRODUCTS.PRODUCT_SHORT_CODE IS 'Product short code, should be numeric, will be used in sms, stk request'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PRODUCTS.PRODUCT_CATEGORY IS 'product category, FLEX, FIXED, only one flex will be allowed in one type'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PRODUCTS.ERP_PRODUCT_CODE IS 'ERP product code, if not applicable enter product code'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PRODUCTS.STATUS IS 'product status, Y, S, N, only Y one will be considered'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PRODUCTS.UNIT_VALUE IS 'product unit value, it should be in lowest denamination of currency in case of india it will be in paisa'
/


--
-- PROCESS_STATUS  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.PROCESS_STATUS ( PROCESS_ID VARCHAR2(20 BYTE) NULL, START_DATE DATE NULL, SCHEDULER_STATUS VARCHAR2(2 BYTE) NULL, EXECUTED_UPTO DATE NULL, EXECUTED_ON DATE NULL, EXPIRY_TIME NUMBER(10) NULL, BEFORE_INTERVAL NUMBER(10) NULL, DESCRIPTION VARCHAR2(100 BYTE) NULL, NETWORK_CODE VARCHAR2(2 BYTE) DEFAULT 'NW' NULL ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 64K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PROCESS_STATUS.PROCESS_ID IS 'Unique id for each process'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PROCESS_STATUS.START_DATE IS 'Date on which schedular process started to execute'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PROCESS_STATUS.SCHEDULER_STATUS IS 'Status of the schedular process: Underprocess (U),Complete (C), Once locked by one process, cannot be used by another process'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PROCESS_STATUS.EXECUTED_UPTO IS 'till which date process has been executed'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PROCESS_STATUS.EXECUTED_ON IS 'date on which process was executed'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PROCESS_STATUS.EXPIRY_TIME IS 'Expiary time of the
process, if it is marked under process then it treaded as completed after
this interval, in minutes'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PROCESS_STATUS.BEFORE_INTERVAL IS 'How many minutes older
records/files picked for processing'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.PROCESS_STATUS.DESCRIPTION IS 'Description or Name of the
process'
/


