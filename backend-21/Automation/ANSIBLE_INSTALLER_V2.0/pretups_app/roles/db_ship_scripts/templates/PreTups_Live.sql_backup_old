--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.USER_TRANSFER_COUNTS ( USER_ID VARCHAR2(15 BYTE) NULL, DAILY_IN_COUNT NUMBER(16) NULL, DAILY_IN_VALUE NUMBER(20) NULL, WEEKLY_IN_COUNT NUMBER(16) NULL, WEEKLY_IN_VALUE NUMBER(20) NULL, MONTHLY_IN_COUNT NUMBER(16) NULL, MONTHLY_IN_VALUE NUMBER(20) NULL, DAILY_OUT_COUNT NUMBER(16) NULL, DAILY_OUT_VALUE NUMBER(20) NULL, WEEKLY_OUT_COUNT NUMBER(16) NULL, WEEKLY_OUT_VALUE NUMBER(20) NULL, MONTHLY_OUT_COUNT NUMBER(16) NULL, MONTHLY_OUT_VALUE NUMBER(20) NULL, OUTSIDE_DAILY_IN_COUNT NUMBER(16) NULL, OUTSIDE_DAILY_IN_VALUE NUMBER(20) NULL, OUTSIDE_WEEKLY_IN_COUNT NUMBER(16) NULL, OUTSIDE_WEEKLY_IN_VALUE NUMBER(20) NULL, OUTSIDE_MONTHLY_IN_COUNT NUMBER(16) NULL, OUTSIDE_MONTHLY_IN_VALUE NUMBER(20) NULL, OUTSIDE_LAST_IN_TIME DATE NULL, LAST_IN_TIME DATE NULL, LAST_OUT_TIME DATE NULL, OUTSIDE_LAST_OUT_TIME DATE NULL, OUTSIDE_DAILY_OUT_COUNT NUMBER(16) NULL, OUTSIDE_DAILY_OUT_VALUE NUMBER(20) NULL, OUTSIDE_WEEKLY_OUT_COUNT NUMBER(16) NULL, OUTSIDE_WEEKLY_OUT_VALUE NUMBER(20) NULL, OUTSIDE_MONTHLY_OUT_COUNT NUMBER(16) NULL, OUTSIDE_MONTHLY_OUT_VALUE NUMBER(20) NULL, DAILY_SUBSCRIBER_OUT_COUNT NUMBER(16) NULL, DAILY_SUBSCRIBER_OUT_VALUE NUMBER(20) NULL, WEEKLY_SUBSCRIBER_OUT_COUNT NUMBER(16) NULL, WEEKLY_SUBSCRIBER_OUT_VALUE NUMBER(20) NULL, MONTHLY_SUBSCRIBER_OUT_COUNT NUMBER(16) NULL, MONTHLY_SUBSCRIBER_OUT_VALUE NUMBER(20) NULL, LAST_TRANSFER_ID VARCHAR2(20 BYTE) NULL, LAST_TRANSFER_DATE DATE NULL, CONSTRAINT PK_USER_TRANSFER_COUNTS PRIMARY KEY (USER_ID) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 384K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 2M NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
