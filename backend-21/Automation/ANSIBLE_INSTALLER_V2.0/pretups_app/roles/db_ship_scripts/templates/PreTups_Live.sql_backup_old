-- C2S_SUMMARY_DAILY  (Table) 
--
CREATE TABLE {{ PRETUPS_SCHEMA_USER_NAME }}.C2S_SUMMARY_DAILY ( TRANS_DATE DATE NULL, SENDER_NETWORK_CODE VARCHAR2(2 BYTE) NOT NULL, RECEIVER_NETWORK_CODE VARCHAR2(2 BYTE) NOT NULL, SERVICE_TYPE VARCHAR2(10 BYTE) NOT NULL, SUB_SERVICE_TYPE VARCHAR2(10 BYTE) NOT NULL, TOTAL_TRANS_COUNT NUMBER(16) NOT NULL, TOTAL_TRANS_AMOUNT NUMBER(20) NULL, FAIL_COUNT NUMBER(16) NOT NULL, FAIL_AMOUNT NUMBER(20) NULL, CONSTRAINT PK_C2S_SUMMARY_DLY PRIMARY KEY (TRANS_DATE, SENDER_NETWORK_CODE, RECEIVER_NETWORK_CODE, SERVICE_TYPE, SUB_SERVICE_TYPE) USING INDEX TABLESPACE {{ P_C2SINDX1_TS_NAME }} PCTFREE 10 INITRANS 2 MAXTRANS 255 STORAGE ( INITIAL 64K NEXT 64M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 ) ) TABLESPACE {{ PRTP_DATA_TS_NAME }} PCTUSED 0 PCTFREE 10 INITRANS 1 MAXTRANS 255 STORAGE ( INITIAL 128K NEXT 16M MINEXTENTS 1 MAXEXTENTS 2147483645 PCTINCREASE 0 BUFFER_POOL DEFAULT ) LOGGING NOCOMPRESS NOCACHE NOPARALLEL MONITORING
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.C2S_SUMMARY_DAILY.TRANS_DATE IS 'transaction date'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.C2S_SUMMARY_DAILY.SENDER_NETWORK_CODE IS 'network code of the sender'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.C2S_SUMMARY_DAILY.RECEIVER_NETWORK_CODE IS 'network code of the receiver'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.C2S_SUMMARY_DAILY.SERVICE_TYPE IS 'service type includes PRE or POST'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.C2S_SUMMARY_DAILY.SUB_SERVICE_TYPE IS 'sub service type includes C, VG and CVG'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.C2S_SUMMARY_DAILY.TOTAL_TRANS_COUNT IS 'total number of transactions'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.C2S_SUMMARY_DAILY.TOTAL_TRANS_AMOUNT IS 'total transaction amount'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.C2S_SUMMARY_DAILY.FAIL_COUNT IS 'number of failures'
/

COMMENT ON COLUMN {{ PRETUPS_SCHEMA_USER_NAME }}.C2S_SUMMARY_DAILY.FAIL_AMOUNT IS 'failure amount'
/


