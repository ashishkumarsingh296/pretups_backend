UPDATE ROLES
SET ROLE_NAME='Change Self PIN', GROUP_NAME='Channel Users', STATUS='Y', ROLE_TYPE='A', FROM_HOUR=NULL, TO_HOUR=NULL, GROUP_ROLE='N', APPLICATION_ID='1', GATEWAY_TYPES='WEB', ROLE_FOR='B', IS_DEFAULT='N', IS_DEFAULT_GROUPROLE='N', ACCESS_TYPE='B', SUB_GROUP_NAME='Sub_Group', SUB_GROUP_ROLE='N', VIEW_ROLES='Y'
WHERE ROLE_CODE='CHANGESELFPIN' AND DOMAIN_TYPE='OPERATOR';



