<script> 
function toggle() {
	var ele = document.getElementById("rechargeStatus");
	var text = document.getElementById("display");
	if(ele.style.display == "block") {
    		ele.style.display = "none";
		text.innerHTML = "show";
  	}
	else {
		ele.style.display = "block";
		text.innerHTML = "hide";
	}
} 
