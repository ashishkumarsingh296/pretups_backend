	<build>

		<finalName>inter</finalName>

		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>true</filtering>
				<excludes>

					<exclude>Constants.props</exclude>
					<exclude>LogConfig.props</exclude>
					<exclude>ESAPI.properties</exclude>
					<exclude>KPIConfig.props</exclude>
					<exclude>MessageResources.properties</exclude>
					<exclude>Messages.properties</exclude>
					<exclude>QueryFile.props</exclude>
					<exclude>Reports_label.properties</exclude>
					<exclude>validation.properties</exclude>
					<exclude>configfiles/**/*.*</exclude>
					<exclude>template/**/*.*</exclude>
					<exclude>*.*</exclude>
				</excludes>
			</resource>
		</resources>
		<pluginManagement>
			<plugins>
				<!-- To disable creation of META-INF/maven files -->

				<plugin>
					<groupId>org.codehaus.mojo</groupId>
					<artifactId>tomcat-maven-plugin</artifactId>
					<version>1.1</version>
					<dependencies>
						<dependency>
							<groupId>org.codehaus.mojo</groupId>
							<artifactId>tomcat-maven-plugin</artifactId>
							<version>1.1</version>
						</dependency>
					</dependencies>
				</plugin>

				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-jar-plugin</artifactId>
					<version>2.6</version>
					<configuration>

						<archive>
							<addMavenDescriptor>
								false
							</addMavenDescriptor>
						</archive>
					</configuration>
				</plugin>

				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-compiler-plugin</artifactId>
					<version>3.1</version>
					<configuration>
						<source>1.8</source>
						<target>1.8</target>
						<encoding>Cp1252</encoding>
						<verbose>true</verbose>
						<!--<fork>true</fork> -->
						<!-- <executable>${JAVA_1_6_HOME}/bin/javac</executable> -->

						<excludes>

							<exclude>simulator/**/*.java</exclude>
							<exclude>loadtest/**/*.java</exclude>
							<exclude>ussd/**/*.java</exclude>
							<exclude>EXTGW/**/*.java</exclude>
							<exclude>com/btsl/**/*.java</exclude>
							<exclude>com/client/**/*.java</exclude>
							<exclude>com/inet/**/*.*</exclude>
							<exclude>com/inter/al*/**/*.java</exclude>
							<exclude>com/inter/bank/**/*.java</exclude>
							<exclude>com/inter/billpayment/**/*.java</exclude>
							<exclude>com/inter/cboss/**/*.java</exclude>
							<exclude>com/inter/citipg/**/*.java</exclude>
							<exclude>com/inter/comversetg/**/*.java</exclude>
							<exclude>com/inter/comversetr/**/*.java</exclude>
							<exclude>com/inter/cs5*/**/*.java</exclude>
							<exclude>com/inter/eri*/**/*.java</exclude>
							<exclude>com/inter/ferm*/**/*.java</exclude>
							<exclude>com/inter/ethopiabillpayment/**/*.java</exclude>
							<exclude>com/inter/huawei*/**/*.java</exclude>
							<exclude>com/inter/iat/**/*.java</exclude>
							<exclude>com/inter/kan*/**/*.java</exclude>
							<exclude>com/inter/mobi*/**/*.java</exclude>
							<exclude>com/inter/nokia/**/*.java</exclude>
							<exclude>com/inter/meditel/**/*.java</exclude>
							<exclude>com/inter/postvfe/**/*.java</exclude>
							<exclude>com/inter/postonline/**/*.java</exclude>
							<exclude>com/inter/post/**/*.java</exclude>
							<exclude>com/inter/sie*/**/*.java</exclude>
							<exclude>com/inter/telnet/**/*.java</exclude>
							<exclude>com/inter/socket/**/*.java</exclude>
							<exclude>com/inter/uganda_webservices/**/*.java</exclude>
							<exclude>com/inter/vastrixtg/**/*.java</exclude>
							<exclude>com/inter/vasthoml/**/*.java</exclude>
							<exclude>com/inter/vodafoneghana*/**/*.java</exclude>
							<exclude>com/inter/zteethopia/**/*.java</exclude>
							<exclude>com/inter/zteobo/**/*.java</exclude>
							<exclude>com/inter/zteoml/**/*.java</exclude>
							<exclude>com/inter/zteoci/**/*.java</exclude>
							<exclude>com/inter/zteogb/**/*.java</exclude>
							<exclude>com/inter/zteogn/**/*.java</exclude>
							<exclude>com/inter/zteoni/**/*.java</exclude>
							<exclude>com/inter/module/Handler*.java</exclude>
							<exclude>com/inter/idea/**/*.java</exclude>
							<exclude>com/inter/claro*/**/*.java</exclude>
							<exclude>com/inter/zteobw/**/*.java</exclude>
							<exclude>com/txn/**/*.java</exclude>
							<exclude>com/web/**/*.java</exclude>
							<exclude>com/selftopup/**/*.java</exclude>
							<exclude>com/restapi/**/*.java</exclude>



