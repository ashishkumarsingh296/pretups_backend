PROCESS_ID=TRANS_REV_DASHBOARD
MAX_ROWS=65000
FILE_SUBSTR_DATE_FORMAT=_ddMMyy_hhmmss_
DATE_FORMAT=dd/MM/yyyy
DATE_TIME_FORMAT=dd/MM/yy hh:mm:ss





TRANS_REV_DASHBOARD_DIR=<Tomcat-path>/logs/csvreports
TRANS_REV_DASHBOARD_HEADER=Location, Month, Total Reversal Request Received, Total Reversal Request successfully processed, Total Reversal Request Failed, Total Reversal MRP, Total Reversal Customer Debit amount, Total Reversal Retailer credit Amount
TRANS_REV_DASHBOARD_QRY=SELECT  ROW_NUMBER() OVER (ORDER BY to_char( d.TRANS_DATE,'MON')),u.NETWORK_CODE||','||to_char(d.TRANS_DATE,'MON')||','||TO_CHAR(SUM(d.TRANSACTION_COUNT+ d.FAILURE_COUNT))||','||SUM(d.TRANSACTION_COUNT )||','||SUM(d.FAILURE_COUNT)||','||TO_CHAR(SUM(d.TRANSACTION_AMOUNT/100.00))||','||TO_CHAR(SUM(d.SENDER_TRANSFER_AMOUNT/100.00))||','||TO_CHAR(SUM(d.RECEIVER_CREDIT_AMOUNT/100.00))FROM DAILY_C2S_TRANS_DETAILS d,USERS u WHERE d.SERVICE_TYPE='RCREV' AND d.USER_ID=u.USER_ID AND EXTRACT(month from SYSDATE) - EXTRACT(month from d.TRANS_DATE) < ?  GROUP BY  to_char(d.TRANS_DATE,'MON'),u.NETWORK_CODE 
TRANS_REV_DASHBOARD_EXT=.csv
TRANS_REV_DASHBOARD_PREFIX_NAME=TRANS_REV_DASHBOARD
TRANS_REV_DASHBOARD_TEMP_TBL=TEMP_TRANS_REV_DASHBOARD
TRANS_REV_DASHBOARD_DYN_ARG=1
TRANS_REV_DASHBOARD_LAST_N_MONTHS=1
