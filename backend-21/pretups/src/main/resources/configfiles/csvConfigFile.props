OTHER_MF_C2C_TRANSFER_DETAILS_HEADER=Owner user, Owner MSISDN, Parent User, Parent MSISDN, From User, From MSISDN, To user, To MSISDN, Transfer ID, To owner Geography, Transfer sub type, Transfer Date, Product,  Status, Sender Category, Receiver category, Source, To user geography, From user geography, From owner geography, Requested value, Payable value, Net Payable value, MRP, Commission value, Receiver Credit, Sender Debit, Tax1, Tax2, Tax3
OTHER_MF_C2C_TRANSFER_DETAILS_QRY=SELECT rownum, REPLACE(OU.user_name,',',' ')||','||OU.msisdn ||','|| REPLACE(PU.user_name,',',' ') ||','|| PU.msisdn ||','|| FU.user_name ||','||  CTRF.msisdn  ||','||  REPLACE(TU.user_name,',',' ') ||','||  CTRF.to_msisdn  ||','|| CTRF.transfer_id ||','||   REPLACE(TWGD.grph_domain_name,',',' ') ||','||  L.lookup_name  ||','||  to_char(CTRF.transfer_date,'dd/mm/yy') ||','|| P.product_name ||','||  L1.lookup_name ||','||  SEND_CAT.category_name  ||','||  REC_CAT.category_name ||','|| CTRF.SOURCE ||','||  REPLACE(TUGD.grph_domain_name,',',' ') ||','||  REPLACE(FUGD.grph_domain_name,',',' ') ||','||  REPLACE(FWGD.grph_domain_name,',',' ') ||','||(CTI.required_quantity/100) ||','||  (CTI.payable_amount/100) ||','||  (CTI.net_payable_amount/100) ||','||  (CTI.mrp/100) ||','||  (CTI.commision_quantity/100) ||','|| (CTI.receiver_credit_quantity/100) ||','|| (CTI.sender_debit_quantity/100) ||','|| (CTI.tax1_value/100)||','|| (CTI.tax2_value/100)||','|| (CTI.tax3_value/100) FROM CHANNEL_TRANSFERS CTRF, CHANNEL_TRANSFERS_ITEMS CTI,USERS FU,USERS TU,PRODUCTS P,LOOKUPS L, LOOKUPS L1,  CATEGORIES SEND_CAT, CATEGORIES REC_CAT,USER_GEOGRAPHIES UG,USERS OU,USER_GEOGRAPHIES UGW, GEOGRAPHICAL_DOMAINS TUGD, GEOGRAPHICAL_DOMAINS TWGD, GEOGRAPHICAL_DOMAINS FUGD,GEOGRAPHICAL_DOMAINS FWGD, USER_GEOGRAPHIES FUG,USER_GEOGRAPHIES FUGW,USERS PU WHERE CTRF.TYPE = 'C2C'  AND CTRF.transfer_date =trunc(sysdate-1)  AND SEND_CAT.domain_code <>'MF' AND CTRF.control_transfer<>'A' AND CTRF.sender_category_code = SEND_CAT.category_code AND CTRF.receiver_category_code = REC_CAT.category_code AND FU.user_id = CTRF.from_user_id AND TU.user_id =CTRF.to_user_id AND FU.owner_id=FUGW.user_id AND FUGW.grph_domain_code=FWGD.grph_domain_code AND FU.user_id=FUG.user_id AND FUG.grph_domain_code=FUGD.grph_domain_code AND CTRF.transfer_id = CTI.transfer_id AND CTI.product_code = P.product_code AND FU.parent_id=PU.user_id(+) AND OU.user_id=FU.owner_id AND UGW.user_id=OU.user_id AND UGW.grph_domain_code=TWGD.grph_domain_code AND L.lookup_type ='TRFT' AND L.lookup_code = CTRF.transfer_sub_type AND CTRF.status = 'CLOSE' AND L1.lookup_code = CTRF.status AND L1.lookup_type = 'CTSTA' AND CTRF.to_user_id = UG.user_id AND UG.grph_domain_code=TUGD.grph_domain_code 
OTHER_MF_C2C_TRANSFER_DETAILS_EXT=.csv
OTHER_MF_C2C_TRANSFER_DETAILS_PREFIX_NAME=OTHER_MF_C2C_TRANSFER_DETAILS
OTHER_MF_C2C_TRANSFER_DETAILS_TEMP_TBL=TEMP_OTH_MF_C2C_TRF_DETAILS
OTHER_MF_C2C_TRANSFER_DETAILS_DYN_ARG=0


ADDITIONAL_COMMISSION_DETAILS_DIR=<Tomcat-path>/logs/csvreports/
ADDITIONAL_COMMISSION_DETAILS_HEADER=Additional Commission ID, Transaction ID, User name, User msisdn, User Category, User Geography, Parent name, Parent msisdn, Parent Category, Parent Geography, Owner name, Owner msisdn, Owner category, Owner Geography, Grand Parent name, Grand Parent msisdn, Grand Parent Category, Grand Parent Geography, Service, Receiver msisdn, Additional commission, Rate, Transfer Value
ADDITIONAL_COMMISSION_DETAILS_QRY=SELECT ADJ.adjustment_id ||','|| ADJ.reference_id ||','|| REPLACE(U.user_name,',',' ') ||','|| U.msisdn ||','|| C.category_name ||','||  REPLACE(GD.grph_domain_name,',',' ') ||','||  REPLACE(PU.user_name,',',' ') ||','|| PU.msisdn ||','|| PC.category_name ||','|| REPLACE(PGD.grph_domain_name,',',' ') ||','||  REPLACE(OU.user_name,',',' ') ||','|| OU.msisdn ||','|| OC.category_name ||','|| REPLACE(OGD.grph_domain_name,',',' ') ||','||   REPLACE(GU.user_name,',',' ') ||','|| GU.msisdn ||','|| GC.category_name ||','|| REPLACE(GGD.grph_domain_name,',',' ') ||','||  ST.NAME ||','|| CST.receiver_msisdn ||','|| (ADJ.margin_amount/100) ||','|| ADJ.margin_rate ||','|| (CST.transfer_value/100) FROM ADJUSTMENTS ADJ, USERS U, C2S_TRANSFERS CST, USER_GEOGRAPHIES UG,  GEOGRAPHICAL_DOMAINS GD,CATEGORIES C, USERS PU, USERS OU, CATEGORIES PC, CATEGORIES OC, USER_GEOGRAPHIES PUG, GEOGRAPHICAL_DOMAINS PGD, USER_GEOGRAPHIES OUG, GEOGRAPHICAL_DOMAINS OGD,SERVICE_TYPE ST,USERS GU, USER_GEOGRAPHIES GUG, CATEGORIES GC,GEOGRAPHICAL_DOMAINS GGD  WHERE ADJ.user_category = C.category_code AND C.domain_code ='MF' AND ADJ.user_id=U.user_id AND CST.transfer_id=SUBSTR(ADJ.adjustment_id, 1,LENGTH(ADJ.adjustment_id)-1) AND CST.transfer_id=ADJ.REFERENCE_ID AND UG.grph_domain_code = GD.grph_domain_code AND UG.user_id=U.user_id AND PUG.grph_domain_code = PGD.grph_domain_code AND PUG.user_id=PU.user_id AND PU.user_id=CASE U.parent_id WHEN 'ROOT' THEN U.user_id ELSE U.parent_id END AND PU.category_code=PC.category_code AND OUG.grph_domain_code = OGD.grph_domain_code AND OUG.user_id=OU.user_id AND OU.user_id=U.owner_id AND OU.category_code=OC.category_code AND GUG.grph_domain_code = GGD.grph_domain_code AND GUG.user_id=GU.user_id AND GU.user_id=CASE PU.parent_id WHEN 'ROOT' THEN PU.user_id ELSE PU.parent_id END AND GU.category_code=GC.category_code AND ADJ.SERVICE_TYPE=ST.SERVICE_TYPE AND TRUNC(ADJ.created_on)=trunc(sysdate-1) 
ADDITIONAL_COMMISSION_DETAILS_EXT=.csv
ADDITIONAL_COMMISSION_DETAILS_PREFIX_NAME=ADDITIONAL_COMMISSION_DETAILS
ADDITIONAL_COMMISSION_DETAILS_TEMP_TBL=TEMP_ADDITIONAL_COM_DETAILS
ADDITIONAL_COMMISSION_DETAILS_DYN_ARG=0

DEL_REJ_CHNL_USER_LIST_DIR=<Tomcat-path>/logs/csvreports/
DEL_REJ_CHNL_USER_LIST_HEADER=Deleted/rejected on, Deleted/Rejected by, User name, Mobile number,	Status,	Contact person,	User category, Geographical domain
DEL_REJ_CHNL_USER_LIST_QRY=SELECT TO_CHAR (U.modified_on, 'dd/mm/yy hh24:mi:ss')||','|| REPLACE(U1.user_name,',',' ') ||','|| REPLACE(U.user_name,',',' ') ||','||U.msisdn||','|| L.lookup_name ||','|| REPLACE(U.contact_person,',',' ') ||','|| CAT.category_name||','|| REPLACE(GD.grph_domain_name,',',' ') FROM USERS U, LOOKUPS L, CATEGORIES CAT, USER_GEOGRAPHIES UG, GEOGRAPHICAL_DOMAINS GD, USERS U1 WHERE TRUNC(U.modified_on)= trunc(sysdate -1) AND U.status IN ('N', 'C', 'DR') AND L.lookup_code = U.status AND L.lookup_type = 'URTYP' AND U.category_code = CAT.category_code AND U.MODIFIED_by=U1.user_id AND U.user_id = UG.user_id AND UG.grph_domain_code = GD.grph_domain_code order by L.lookup_name 
DEL_REJ_CHNL_USER_LIST_EXT=.csv
DEL_REJ_CHNL_USER_LIST_PREFIX_NAME=DELETE_REJECT_CHNL_USER_LIST
DEL_REJ_CHNL_USER_LIST_TEMP_TBL=TEMP_DEL_REJ_CHNL_USER_LIST
DEL_REJ_CHNL_USER_LIST_DYN_ARG=0







