OTHER_MF_USER_BALANCE_HEADER=Balance Date, User name, User mobile, Domain name, Category name, User code, User geographies, Product name, Parent name, Owner name, User balance, Sum of Child balances
OTHER_MF_USER_BALANCE_QRY=Select rownum,Y.data from(SELECT to_char(sysdate-1,'dd/mm/yy')||','|| (REPLACE(X1.user_name,',',' ')||','|| X1.msisdn||','|| X1.domain_name||','||X1.user_category||','|| X1.user_code||','|| REPLACE(X1.user_geography,',',' ')||','||X1.product_name||','||REPLACE(X1.parent_name,',',' ')||','|| REPLACE(X1.owner_name,',',' ')||','||X1.user_balance/100||','|| nvl(SUM(X1.agent_balance), 0)/100) Data  FROM (SELECT X.user_id, X.user_name, X.msisdn, X.domain_name, X.user_category, X.user_code,  X.user_geography,X.product_name,  X.parent_name, X.owner_name, nvl(X.user_balance_daily,X.user_balance) user_balance,UD.category_code,UD.parent_id,    CD.category_type , nvl(BAL.balance,UBAL.balance) agent_balance FROM (SELECT U.user_id,U.user_name user_name, U.msisdn msisdn, CAT.category_name user_category,    U.user_code user_code,  GD.grph_domain_name user_geography, P.product_name product_name, (CASE U.parent_id WHEN 'ROOT' THEN U.user_name ELSE PU.user_name END) parent_name,     OU.user_name owner_name, BAL.balance user_balance_daily, UBAL.balance user_balance, D.DOMAIN_NAME     FROM users U, users OU, users PU,categories CAT, user_daily_balances BAL, user_balances UBAL, user_geographies UG, geographical_domains GD,products P,domains D     WHERE U.category_code = CAT.category_code AND CAT.domain_code=D.domain_code AND CAT.domain_code <> 'MF' AND BAL.balance_date(+) = trunc(sysdate-1)     AND nvl(BAL.balance_date, TRUNC(UBAL.daily_balance_updated_on)) = trunc(sysdate-1) AND U.user_id = UBAL.user_id AND BAL.user_id(+)= UBAL.user_id     AND BAL.product_code(+)=UBAL.product_code AND BAL.network_code(+)=UBAL.network_code     AND PU.user_id = (case U.parent_id when 'ROOT' then U.user_id else U.parent_id end)     AND U.owner_id=OU.user_id AND UBAL.product_code= P.product_code AND U.user_id = UG.user_id     AND UG.grph_domain_code = GD.grph_domain_code AND NVL(U.status,'X') !='N' ) X, users UD ,categories CD, user_balances UBAL,user_daily_balances BAL      WHERE X.User_id =UD.parent_id (+)  AND UD.category_code =CD.category_code (+)  AND UD.USER_ID=UBAL.USER_ID      AND BAL.balance_date(+) =trunc(sysdate-1) AND BAL.balance_date(+) =trunc(sysdate-1)     AND BAL.user_id(+) = UBAL.user_id  AND BAL.product_code(+)=UBAL.product_code AND BAL.network_code(+)=UBAL.network_code ) X1     WHERE NVL(X1.category_type,'AGENT' )=CASE NVL(X1.category_type,'AGENT' ) WHEN 'AGENT' THEN 'AGENT' ELSE X1.category_type END     GROUP BY X1.domain_name,X1.user_category,X1.user_id, X1.user_name, X1.msisdn, X1.user_code, X1.user_geography,X1.product_name,      X1.parent_name, X1.owner_name, X1.user_balance,x1.parent_id)Y 
