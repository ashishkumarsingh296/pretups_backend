id =UD.parent_id (+)  AND UD.category_code =CD.category_code (+)  AND UD.USER_ID=UBAL.USER_ID      AND BAL.balance_date(+) =trunc(sysdate-1) AND BAL.balance_date(+) =trunc(sysdate-1)     AND BAL.user_id(+) = UBAL.user_id  AND BAL.product_code(+)=UBAL.product_code AND BAL.network_code(+)=UBAL.network_code ) X1     WHERE NVL(X1.category_type,'AGENT' )=CASE NVL(X1.category_type,'AGENT' ) WHEN 'AGENT' THEN 'AGENT' ELSE X1.category_type END     GROUP BY X1.domain_name,X1.user_category,X1.user_id, X1.user_name, X1.msisdn, X1.user_code, X1.user_geography,X1.product_name,      X1.parent_name, X1.owner_name, X1.user_balance,x1.parent_id)Y 
