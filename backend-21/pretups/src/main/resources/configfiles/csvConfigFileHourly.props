#PROCESS_ID=MF_EXT_USR_LIST, OTHER_MF_EXT_USR_LIST, OTHER_MF_USER_BALANCE, MF_USER_BALANCE, RC_C2S_TRANSFER_DETAILS, PPB_C2S_TRANSFER_DETAILS, EVD_C2S_TRANSFER_DETAILS, OTHER_MF_C2C_TRANSFER_DETAILS, MF_C2C_TRANSFER_DETAILS, ADDITIONAL_COMMISSION_DETAILS, DEL_REJ_CHNL_USER_LIST
PROCESS_ID=USR_BAL_TR_REPORT,EVD,BATCHOPERATION
MAX_ROWS=65000
FILE_SUBSTR_DATE_FORMAT=_ddMMyy_hhmmss_
DATE_FORMAT=dd/MM/yyyy
DATE_TIME_FORMAT=dd/MM/yy hh:mm:ss

USR_BAL_TR_REPORT_DIR=<Tomcat-path>/logs/csvreports/USER BALANCE/
USR_BAL_TR_REPORT_HEADER=User name, External Code, Balance, Balance Type
USR_BAL_TR_REPORT_QRY=SELECT rownum,a.USER_NAME||','||a.EXTERNAL_CODE||','||b.BALANCE||','||b.BALANCE_TYPE FROM USERS a,USER_BALANCES b  WHERE b.DAILY_BALANCE_UPDATED_ON between ? and ? AND a.USER_ID=b.USER_ID order by b.BALANCE_TYPE  
USR_BAL_TR_REPORT_EXT=.csv
USR_BAL_TR_REPORT_PREFIX_NAME=USERBALANCE_TRACKINGREPORT

BATCHOPERATION_DIR=<Tomcat-path>/logs/csvreports/BATCH OPERATION/
BATCHOPERATION_HEADER=Sender Msisdn, Service Type, Sender Transfer Value
BATCHOPERATION_QRY=SELECT rownum,SENDER_MSISDN||','||SERVICE_TYPE||','||SENDER_TRANSFER_VALUE FROM C2S_TRANSFERS  WHERE transfer_date_time between ? and ? AND SERVICE_TYPE IN ('CS','CLA','CLF')
BATCHOPERATION_EXT=.csv
BATCHOPERATION_PREFIX_NAME=BATCHOPERATION_TRACKINGREPORT

EVD_DIR=<Tomcat-path>/logs/csvreports/EVD/
EVD_HEADER=Sender Msisdn, Serial Number, Transfer Date
EVD_QRY=SELECT rownum,SENDER_MSISDN||','||SERIAL_NUMBER||','||TRANSFER_DATE FROM C2S_TRANSFERS  WHERE transfer_date_time between ? and ? AND SERVICE_TYPE IN ('EVD')
EVD_EXT=.csv
EVD_PREFIX_NAME=EVD_TRACKINGREPORT